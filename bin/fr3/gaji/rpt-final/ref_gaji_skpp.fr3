<TfrxReport Version="3.22" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="1273" PreviewOptions.MDIChild="True" PreviewOptions.Modal="False" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="40575.6801940393" ReportOptions.Description.Text="" ReportOptions.LastChange="40575.6840020949" ScriptLanguage="PascalScript" ScriptText.Text="VAR DEFQUERY, QGAJI, SNAMANIP: STRING;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetTerusan(AktifTgl: TDateTime; Thn, Bln: String): String;&#13;&#10;var Bulan: String;&#13;&#10;    nTahun, AktifThn, AktifBln, nBln: Integer;&#13;&#10;begin&#13;&#10;       Result:= '0';&#13;&#10;       AktifThn:= YearOf(AktifTgl);&#13;&#10;       if (AktifThn&#60;=1900) then exit;&#13;&#10;       nTahun:= StrToInt(Thn) - AktifThn;&#13;&#10;       nTahun:= nTahun * 12;&#13;&#10;       if (nTahun&#60;0) then exit;&#13;&#10;       AktifBln:= MonthOf(AktifTgl);&#13;&#10;       nBln:= (nTahun - AktifBln) + StrToInt(Bln);&#13;&#10;       Result:= IntToStr(nBln);&#13;&#10;end;&#13;&#10;&#13;&#10;function GetQueryHistory(BLN1, BLN2, THN1, THN2: INTEGER): String;&#13;&#10;var B,I, J, N, M, TH, TMPTH, THBEFORE: integer;&#13;&#10;       W, S, TRES, RES, SBLN, STHN: STRING;&#13;&#10;begin&#13;&#10;       TRES:= '';&#13;&#10;       IF (THN1=THN2) THEN&#13;&#10;               N:= BLN2 - BLN1 + 1&#13;&#10;       ELSE IF (THN1&#60;THN2) THEN&#13;&#10;               N:= ((12 * (THN2 - THN1)) + BLN2) - BLN1&#13;&#10;       ELSE N:= 0;&#13;&#10;       M:= 12 - BLN1;&#13;&#10;       J:= 0;&#13;&#10;       TH:= THN1;&#13;&#10;       THBEFORE:= THN1;&#13;&#10;       IF (N&#62;0) THEN&#13;&#10;       BEGIN&#13;&#10;               FOR I:= 0 TO N-1 DO&#13;&#10;               BEGIN&#13;&#10;                       IF (I&#60;=M) THEN&#13;&#10;                       BEGIN&#13;&#10;                               TH:= THN1;&#13;&#10;                               B:= BLN1 + I;&#13;&#10;                       END&#13;&#10;                       ELSE&#13;&#10;                       BEGIN&#13;&#10;                               TH:= THBEFORE + 1;&#13;&#10;                               INC(J);&#13;&#10;                               IF (J&#62;12) THEN&#13;&#10;                               BEGIN&#13;&#10;                                       J:=1;&#13;&#10;                                       THBEFORE:= TH;&#13;&#10;                                       TH:= TH + 1;&#13;&#10;                               END;&#13;&#10;                               B:= J;&#13;&#10;                       END;&#13;&#10;                       IF (I&#60;1) THEN&#13;&#10;                               TRES:= DEFQUERY&#13;&#10;                       ELSE&#13;&#10;                               TRES:= ' UNION ' + #13 + ' ' + DEFQUERY;&#13;&#10;                       SBLN:= '0' + INTTOSTR(B);&#13;&#10;                       SBLN:= COPY(SBLN, LENGTH(SBLN)-1, 2);&#13;&#10;                       STHN:= INTTOSTR(TH);&#13;&#10;                       REPLACESTR(TRES, '{BULAN}', SBLN);&#13;&#10;                       REPLACESTR(TRES, '{TAHUN}', INTTOSTR(TH));&#13;&#10;                       REPLACESTR(TRES, 'NOM=0', 'NOM=' + INTTOSTR(I+1));&#13;&#10;                       RES:= RES + TRES&#13;&#10;               END;&#13;&#10;       END;&#13;&#10;       IF (TRES='') THEN&#13;&#10;       BEGIN&#13;&#10;               RES:= DEFQUERY;&#13;&#10;               SBLN:= '0' + INTTOSTR(BLN1);&#13;&#10;               SBLN:= COPY(SBLN, LENGTH(SBLN)-1, 2);&#13;&#10;               REPLACESTR(RES, '{BULAN}', SBLN);&#13;&#10;               REPLACESTR(RES, '{TAHUN}', INTTOSTR(THN1));&#13;&#10;               REPLACESTR(RES, 'NOM=0', 'NOM=1');&#13;&#10;       END;&#13;&#10;       RESULT:= RES;&#13;&#10;end;&#13;&#10;&#13;&#10;PROCEDURE ADDTAHUN;&#13;&#10;BEGIN&#13;&#10;       COTAHUN1.ITEMS.CLEAR;&#13;&#10;       COTAHUN2.ITEMS.CLEAR;&#13;&#10;       IF (QRYTAHUN.RECORDCOUNT&#62;0) THEN&#13;&#10;       BEGIN&#13;&#10;               QRYTAHUN.FIRST;&#13;&#10;               WHILE NOT QRYTAHUN.EOF DO&#13;&#10;               BEGIN&#13;&#10;                       COTAHUN1.ITEMS.ADD(&#60;QRYTAHUN.&#34;TAHUN&#34;&#62;);&#13;&#10;                       COTAHUN2.ITEMS.ADD(&#60;QRYTAHUN.&#34;TAHUN&#34;&#62;);&#13;&#10;                       QRYTAHUN.NEXT;&#13;&#10;               END;&#13;&#10;               COTAHUN1.ITEMINDEX:= QRYTAHUN.RECORDCOUNT - 1;&#13;&#10;               COTAHUN2.ITEMINDEX:= QRYTAHUN.RECORDCOUNT - 1;&#13;&#10;       END;&#13;&#10;END;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;VAR B1, B2, D1, D2, Q:String;&#13;&#10;       I: Integer;&#13;&#10;       D, C: TDateTime;&#13;&#10;begin&#13;&#10;       QRYGAJI.CLOSE;&#13;&#10;       QRYPGW.CLOSE;&#13;&#10;       B1:= '0' + IntToStr(COBULAN1.ItemIndex + 1);&#13;&#10;       B1:= Copy(B1, Length(B1)-1, 2);&#13;&#10;       I:= COBULAN2.ItemIndex + 1;&#13;&#10;       //if (I&#60;12) then I:= I+1;&#13;&#10;       B2:= '0' + IntToStr(I);&#13;&#10;       B2:= Copy(B2, Length(B2)-1, 2);&#13;&#10;       SNAMANIP:= ENIP.TEXT;&#13;&#10;       IF (ENIP.TEXT&#60;&#62;'') THEN&#13;&#10;       BEGIN&#13;&#10;               ReplaceStr(QRYGAJI.SQL.TEXT, '{NIP}', ENIP.TEXT);&#13;&#10;               ReplaceStr(QRYGAJI.SQL.TEXT, '{NAMA}', ENIP.TEXT);&#13;&#10;               ReplaceStr(QRYPGW.SQL.TEXT, '{NIP}', ENIP.TEXT);&#13;&#10;               ReplaceStr(QRYPGW.SQL.TEXT, '{NAMA}', ENIP.TEXT);&#13;&#10;       END;&#13;&#10;       ReplaceStr(QRYGAJI.SQL.TEXT, '{UNITKD}', UNITKD);&#13;&#10;       QRYPGW.OPEN;&#13;&#10;       QGAJI:= QRYGAJI.SQL.TEXT;&#13;&#10;       Q:= GETQUERYHISTORY(COBULAN1.ItemIndex + 1, COBULAN2.ItemIndex + 1, STRTOINT(COTAHUN1.TEXT), STRTOINT(COTAHUN2.TEXT));&#13;&#10;       QRYTEMP.CLOSE;&#13;&#10;       QRYTEMP.SQL.TEXT:= Q;&#13;&#10;       QRYTEMP.OPEN;&#13;&#10;       //ShowMessage(Q);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;VAR S: STRING;&#13;&#10;begin&#13;&#10;       S:= QGAJI;&#13;&#10;       QRYGAJI.CLOSE;&#13;&#10;       REPLACESTR(S, '{BULAN}', &#60;QRYTEMP.&#34;BLN&#34;&#62;);&#13;&#10;       REPLACESTR(S, '{TAHUN}', &#60;QRYTEMP.&#34;THN&#34;&#62;);&#13;&#10;       QRYGAJI.SQL.TEXT:= S;&#13;&#10;       QRYGAJI.OPEN;&#13;&#10;      // ShowMessage(S);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  qrySKPD.Close;&#13;&#10;  qrySKPD.Params[0].Value:=UnitKd;&#13;&#10;  qrySKPD.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;       QRYTAHUN.OPEN;&#13;&#10;       QRYTEMP.CLOSE;&#13;&#10;       DEFQUERY:= QRYTEMP.SQL.TEXT;&#13;&#10;       ADDTAHUN;&#13;&#10;&#13;&#10;       QRYProperti.Close;&#13;&#10;       QRYProperti.SQL.Text:= 'SELECT * FROM UNITTAHUN WHERE TAHUN=''' + TahunAnggaran + '''' +&#13;&#10;                       ' AND UnitKd=''' + UnitKd + '''';&#13;&#10;       QRYProperti.Open;&#13;&#10;end." PropData="">
