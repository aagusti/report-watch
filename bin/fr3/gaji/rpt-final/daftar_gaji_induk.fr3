<TfrxReport Version="3.22" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="40575,5942467708" ReportOptions.Description.Text="" ReportOptions.LastChange="40586,4310223148" ScriptLanguage="PascalScript" ScriptText.Text="procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetTerusan(AktifTgl: TDateTime; Thn, Bln: String): String;&#13;&#10;var Bulan: String;&#13;&#10;    nTahun, AktifThn, AktifBln, nBln: Integer;&#13;&#10;begin&#13;&#10;       Result:= '0';&#13;&#10;       AktifThn:= YearOf(AktifTgl);&#13;&#10;       if (AktifThn&#60;=1900) then exit;&#13;&#10;       nTahun:= StrToInt(Thn) - AktifThn;&#13;&#10;       nTahun:= nTahun * 12;&#13;&#10;       if (nTahun&#60;0) then exit;&#13;&#10;       AktifBln:= MonthOf(AktifTgl);&#13;&#10;       nBln:= (nTahun - AktifBln) + StrToInt(Bln);&#13;&#10;       Result:= IntToStr(nBln);&#13;&#10;end;&#13;&#10;&#13;&#10;function HasSubUnit(kode_skpd : string): boolean;&#13;&#10;begin&#13;&#10;  qrySub.Close;&#13;&#10;  ReplaceStr(qrySub.SQL.Text, '&#60;unitkd&#62;', kode_skpd);&#13;&#10;  qrySub.Open;&#13;&#10;  Result:= (&#60;qrySub.&#34;sub_x&#34;&#62; &#62; 1);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cbTahun.Items.Clear;&#13;&#10;  while not qryTA.Eof do&#13;&#10;  begin&#13;&#10;    cbTahun.Items.Add( &#60;qryTA.&#34;Tahun&#34;&#62; );&#13;&#10;    qryTA.Next;&#13;&#10;  end;&#13;&#10;  cbTahun.ItemIndex := 0;&#13;&#10;  cbBulan.ItemIndex := MonthOf(Date) - 1;&#13;&#10;  qrySKPD.Close;&#13;&#10;  qrySKPD.Params[0].Value:=UnitKd;&#13;&#10;  qrySKPD.Open;&#13;&#10;&#13;&#10;  cbIncludeSub.Visible:= HasSubUnit(UnitKd);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btLanjutOnClick(Sender: TfrxComponent);&#13;&#10;var strWhere, sTemp: String;&#13;&#10;begin&#13;&#10;&#13;&#10;  qryGaji.Close;&#13;&#10;  strWhere:='(g.Tahun='''+cbTahun.Text +''')';&#13;&#10;&#13;&#10;  stemp:='0'+intToStr(cbBulan.ItemIndex + 1);&#13;&#10;  stemp:=copy(stemp,length(stemp)-1,2);&#13;&#10;  strWhere:=strWhere+'AND (g.bulan='''+ stemp+''')';&#13;&#10;&#13;&#10;  if (cbIncludeSub.Visible and cbIncludeSub.Checked) then&#13;&#10;       strWhere:= strWhere + ' and ((g.unitkd='''+UnitKd+''')or(g.unitkd like '''+unitkd+'%''))'&#13;&#10;  else&#13;&#10;       strWhere:=strWhere+'AND (g.UnitKd='''+UnitKd+''')';&#13;&#10;&#13;&#10;  if edNamaNIP.Text &#60;&#62; '' then&#13;&#10;  begin&#13;&#10;    strWhere:=strWhere + ' AND ((g.nip like ''' + edNamaNIP.Text + '%'') or (g.nama like ''' + edNamaNIP.Text + '%''))';&#13;&#10;  end;&#13;&#10;&#13;&#10;  if rbGaji13.Checked then&#13;&#10;    strWhere:=strWhere+' AND (g.jenis=1)'&#13;&#10;  else if rbGajiTerus.Checked then&#13;&#10;    strWhere:=strWhere+' AND (g.jenis=6)'&#13;&#10;  else&#13;&#10;    strWhere:=strWhere+' AND (g.jenis=0)  and (not g.aktif_kd in(3,4,5,6))';&#13;&#10;&#13;&#10;  ReplaceStr(qryGaji.SQL.Text, '1=1', strWhere);&#13;&#10;  qryGaji.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;       qrySub.Close;&#13;&#10;       qryGaji.Close;&#13;&#10;end." PropData="">
