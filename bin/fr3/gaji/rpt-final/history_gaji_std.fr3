<TfrxReport Version="4.0.11" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="\\PROPERINDO2\Epson LQ-2180 ESC/P 2" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40205.53230125" ReportOptions.Description.Text="" ReportOptions.LastChange="40259.6886241319" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;VAR DEFQUERY: STRING;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetQueryHistory(BLN1, BLN2, THN1, THN2: INTEGER): String;&#13;&#10;var I, J, N, M: integer;&#13;&#10;       W, S, T, RES, B: STRING;&#13;&#10;begin&#13;&#10;       RES:= DEFQUERY;&#13;&#10;       IF (THN1=THN2) THEN&#13;&#10;               N:= BLN2 - BLN1&#13;&#10;       ELSE IF (THN1&#60;THN2) THEN&#13;&#10;               N:= ((12 * (THN2 - THN1) + BLN2) - BLN1&#13;&#10;       ELSE N:= 0;&#13;&#10;       M:= 12 - BLN1;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;VAR B1, B2, D1, D2:String;&#13;&#10;       I: Integer;&#13;&#10;       D, C: TDateTime;&#13;&#10;begin&#13;&#10;       QRYGAJI.CLOSE;&#13;&#10;       B1:= '0' + IntToStr(COBULAN1.ItemIndex + 1);&#13;&#10;       B1:= Copy(B1, Length(B1)-1, 2);&#13;&#10;       I:= COBULAN2.ItemIndex + 1;&#13;&#10;       if (I&#60;12) then I:= I+1;&#13;&#10;       B2:= '0' + IntToStr(I);&#13;&#10;       B2:= Copy(B2, Length(B2)-1, 2);&#13;&#10;       C:= StrToDate(B1 + '/01' + '/' + COTAHUN1.TEXT);&#13;&#10;       D1:= FormatDateTime('yyyy/mm/dd', C);&#13;&#10;       if (I&#60;12) then&#13;&#10;               D:= StrToDate(B2 + '/01/' + COTAHUN2.TEXT)&#13;&#10;       else&#13;&#10;               D:= StrToDate('01/01/' + IntToStr(StrToInt(COTAHUN2.TEXT)+1));&#13;&#10;       D2:= FormatDateTime('yyyy/mm/dd', D-1);&#13;&#10;       IF (ENIP.TEXT&#60;&#62;'') THEN&#13;&#10;       BEGIN&#13;&#10;               ReplaceStr(QRYGAJI.SQL.TEXT, '{NIP}', ENIP.TEXT);&#13;&#10;               ReplaceStr(QRYGAJI.SQL.TEXT, '{NAMA}', ENIP.TEXT + '%');&#13;&#10;       END;&#13;&#10;       ReplaceStr(QRYGAJI.SQL.TEXT, 'AWAL', D1);&#13;&#10;       ReplaceStr(QRYGAJI.SQL.TEXT, 'AKHIR', D2);&#13;&#10;       ReplaceStr(QRYGAJI.SQL.TEXT, '{UNITKD}', UnitKd);&#13;&#10;       QRYGAJI.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;       QRYTAHUN.OPEN;&#13;&#10;       QRYGAJI.CLOSE;&#13;&#10;       DEFQUERY:= QRYGAJI.SQL.TEXT;&#13;&#10;end." PropData="">
