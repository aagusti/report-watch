<TfrxReport Version="3.22" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="1273" PreviewOptions.MDIChild="True" PreviewOptions.Modal="False" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="40575.6457517708" ReportOptions.Description.Text="" ReportOptions.LastChange="40575.6568194792" ScriptLanguage="PascalScript" ScriptText.Text="const&#13;&#10;  Satu      = 'satu ';&#13;&#10;  Belas     = 'belas ';&#13;&#10;  ThousandSeparator = ',';&#13;&#10;&#13;&#10;VAR  Angka     : array[1..9]of string;&#13;&#10;  Satuan3   : array[1..2]of string;&#13;&#10;  Satuan    : array[0..3]of string;&#13;&#10;  SUBTOTAL  : Integer;&#13;&#10;&#13;&#10;PROCEDURE CREATEBILANGAN;&#13;&#10;BEGIN&#13;&#10;       ANGKA[1]:= 'se';&#13;&#10;       ANGKA[2]:= 'dua ';&#13;&#10;       ANGKA[3]:= 'tiga ';&#13;&#10;       ANGKA[4]:= 'empat ';&#13;&#10;       ANGKA[5]:= 'lima ';&#13;&#10;       ANGKA[6]:= 'enam ';&#13;&#10;       ANGKA[7]:= 'tujuh ';&#13;&#10;       ANGKA[8]:= 'delapan ';&#13;&#10;       ANGKA[9]:= 'sembilan ';&#13;&#10;       Satuan3[1]:= 'ratus ';&#13;&#10;       Satuan3[2]:= 'puluh ';&#13;&#10;       Satuan[0]:= '';&#13;&#10;       Satuan[1]:= 'ribu ';&#13;&#10;       Satuan[2]:= 'juta ';&#13;&#10;       Satuan[3]:= 'milyar ';&#13;&#10;END;&#13;&#10;&#13;&#10;function GetTerbilang(FNumber: Integer):string;&#13;&#10;var&#13;&#10;  tmp,tmp2  : string;&#13;&#10;  TStr      : TStringList;&#13;&#10;  i,j       : integer;&#13;&#10;begin&#13;&#10;  TStr:=TStringList.Create;&#13;&#10;  tmp :=format('%0.0n',[strtofloat(inttostr(FNumber))])+ThousandSeparator;&#13;&#10;  while tmp &#60;&#62; '' do&#13;&#10;  begin&#13;&#10;    TStr.Insert(0,copy(tmp,1,pos(ThousandSeparator,tmp)-1));&#13;&#10;    delete(tmp,1,pos(ThousandSeparator,tmp));&#13;&#10;  end;&#13;&#10;  for i:=0 to TStr.Count-1 do&#13;&#10;    TStr.Strings[i] :=format('%0.3d',[strtoint(TStr.Strings[i])]);&#13;&#10;  for i:=TStr.Count-1 downto 0 do&#13;&#10;  begin&#13;&#10;    tmp  :=TStr.Strings[i];&#13;&#10;    for j:=1 to 3 do&#13;&#10;    begin&#13;&#10;      if tmp[j] = '0' then continue;&#13;&#10;      case j of&#13;&#10;        1 : if tmp[j] &#60;&#62; '0' then&#13;&#10;              tmp2 := tmp2 + Angka[strtoint(tmp[j])] + Satuan3[j];&#13;&#10;        2 : case tmp[j] of&#13;&#10;              '1'      : begin&#13;&#10;                           case tmp[j+1] of&#13;&#10;                             '0'      : tmp2 := tmp2 + Angka[strtoint(tmp[j])] + Satuan3[j];&#13;&#10;                             '1'..'9' : tmp2 := tmp2 + Angka[strtoint(tmp[j+1])] + Belas;&#13;&#10;                           end;&#13;&#10;                           break;&#13;&#10;                         end;&#13;&#10;              '2'..'9' : tmp2 := tmp2 + Angka[strtoint(tmp[j])] + Satuan3[j];&#13;&#10;            end;&#13;&#10;        3 : case tmp[j] of&#13;&#10;              '1' : case FNumber of&#13;&#10;                      1         : tmp2 := tmp2 + Satu;&#13;&#10;                      1000..1999: if i = 0 then&#13;&#10;                                    tmp2 := tmp2 + Satu&#13;&#10;                                  else&#13;&#10;                                    tmp2 := tmp2 + Angka[strtoint(tmp[j])];&#13;&#10;                    else&#13;&#10;                      tmp2 :=tmp2 + Satu;&#13;&#10;                    end;&#13;&#10;            else&#13;&#10;              tmp2 := tmp2 + Angka[strtoint(tmp[j])];&#13;&#10;            end;&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;    if strtoint(tmp) &#60;&#62; 0 then&#13;&#10;      tmp2 := tmp2 + Satuan[i];&#13;&#10;  end;&#13;&#10;  TStr.Free;&#13;&#10;  result :=Trim(tmp2);&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;FUNCTION TGLINDONESIA(D:TDATETIME): STRING;&#13;&#10;VAR B,H,T: STRING;&#13;&#10;       I: INTEGER;&#13;&#10;BEGIN&#13;&#10;       I:= MONTHOF(D);&#13;&#10;       CASE I OF&#13;&#10;       1: B:= 'Januari';&#13;&#10;       2: B:= 'Februari';&#13;&#10;       3: B:= 'Maret';&#13;&#10;       4: B:= 'April';&#13;&#10;       5: B:= 'Mei';&#13;&#10;       6: B:= 'Juni';&#13;&#10;       7: B:= 'Juli';&#13;&#10;       8: B:= 'Agustus';&#13;&#10;       9: B:= 'September';&#13;&#10;       10: B:= 'Oktober';&#13;&#10;       11: B:= 'Nopember';&#13;&#10;       12: B:= 'Desember';&#13;&#10;       END;&#13;&#10;       H:= '0' + INTTOSTR(DAYOF(D));&#13;&#10;       H:= COPY(H, LENGTH(H)-1,2);&#13;&#10;       T:= INTTOSTR(YEAROF(D));&#13;&#10;       RESULT:= H + ' ' + B + ' ' + T;&#13;&#10;END;&#13;&#10;&#13;&#10;FUNCTION GETBULAN(D:TDATETIME): STRING;&#13;&#10;VAR B: STRING;&#13;&#10;       I: INTEGER;&#13;&#10;BEGIN&#13;&#10;       I:= MONTHOF(D);&#13;&#10;       CASE I OF&#13;&#10;       1: B:= 'Januari';&#13;&#10;       2: B:= 'Februari';&#13;&#10;       3: B:= 'Maret';&#13;&#10;       4: B:= 'April';&#13;&#10;       5: B:= 'Mei';&#13;&#10;       6: B:= 'Juni';&#13;&#10;       7: B:= 'Juli';&#13;&#10;       8: B:= 'Agustus';&#13;&#10;       9: B:= 'September';&#13;&#10;       10: B:= 'Oktober';&#13;&#10;       11: B:= 'Nopember';&#13;&#10;       12: B:= 'Desember';&#13;&#10;       END;&#13;&#10;       RESULT:= B;&#13;&#10;END;&#13;&#10;&#13;&#10;FUNCTION GETTAHUN(D:TDATETIME): STRING;&#13;&#10;BEGIN&#13;&#10;       RESULT:= INTTOSTR(YEAROF(D));&#13;&#10;END;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;FUNCTION GETBULAT(NN: DOUBLE): INTEGER;&#13;&#10;VAR S: STRING;&#13;&#10;BEGIN&#13;&#10;       S:= FORMATFLOAT('#,##0', NN);&#13;&#10;       S:= COPY(S, LENGTH(S)-1, 2);&#13;&#10;       IF (NN&#60;1) THEN&#13;&#10;               RESULT:= 0&#13;&#10;       ELSE&#13;&#10;       begin&#13;&#10;               IF (strToInt(S)&#62;0) then&#13;&#10;                       RESULT:= 100-STRTOINT(S)&#13;&#10;               else Result:= 0;&#13;&#10;       end;&#13;&#10;END;&#13;&#10;&#13;&#10;procedure TGLMATIOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       TGLMATI.CLEAR;&#13;&#10;       TGLMATI.TEXT:= ': ' + TGLINDONESIA(&#60;QRYGAJI.&#34;TGL&#34;&#62;);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DMPMemo15OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       REPLACESTR(DMPMemo15.TEXT, '&#60;BLN&#62;', GETBULAN(&#60;QRYGAJI.&#34;TGL&#34;&#62;) + ' ' + GETTAHUN(&#60;QRYGAJI.&#34;TGL&#34;&#62;));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF (NOT (QRYUDTUDW.ACTIVE)) THEN QRYUDTUDW.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;VAR W,S,J: STRING;&#13;&#10;begin&#13;&#10;       IF (RBUDW.CHECKED) THEN&#13;&#10;       BEGIN&#13;&#10;               W:='UDW';&#13;&#10;               J:= '2';&#13;&#10;       END&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               W:= 'UDT';&#13;&#10;               J:= '3';&#13;&#10;       END;&#13;&#10;       QRYJENIS.CLOSE;&#13;&#10;       QRYJENIS.SQL.TEXT:= 'SELECT * FROM TBLJENISGAJI WHERE JENIS_KD=''' + W + '''';&#13;&#10;       QRYJENIS.OPEN;&#13;&#10;       S:= ' NIP=''' + &#60;QRYUDTUDW.&#34;NIP&#34;&#62; + ''' AND ' ;&#13;&#10;       QRYGAJI.CLOSE;&#13;&#10;       REPLACESTR(QRYGAJI.SQL.TEXT, '--strWhere AND', S);&#13;&#10;       REPLACESTR(QRYGAJI.SQL.TEXT, '1=1', ' A.JENIS=' + J);// + ' AND A.UNITKD= ''' + UNITKD + '''');&#13;&#10;       QRYGAJI.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DMPMemo56OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       REPLACESTR(DMPMemo56.TEXT, '&#60;tgl&#62;', TGLINDONESIA(&#60;QRYGAJI.&#34;TGLBYR_UDWUDT&#34;&#62;));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;VAR N: Integer;&#13;&#10;       ST: String;&#13;&#10;begin&#13;&#10;       SUBTOTAL:= &#60;QRYGAJI.&#34;GAJI_POKOK&#34;&#62;+&#60;QRYGAJI.&#34;TUNJ_ISTRI&#34;&#62;+&#60;QRYGAJI.&#34;TUNJ_ANAK&#34;&#62;+&#60;QRYGAJI.&#34;TUNJ_BERAS&#34;&#62;+&#13;&#10;                       &#60;QRYGAJI.&#34;TUNJ_UMUM&#34;&#62;+&#60;QRYGAJI.&#34;TUNJ_UMUM_TAMB&#34;&#62;+&#60;QRYGAJI.&#34;TUNJ_JAB_STRUKTUR&#34;&#62;+&#60;QRYGAJI.&#34;TUNJ_JAB_FUNGSI&#34;&#62;;&#13;&#10;       N:= SUBTOTAL + GETBULAT(SUBTOTAL);&#13;&#10;       ST:= UPPERCASE(GETTERBILANG(N)) + ' RUPIAH';&#13;&#10;       HURUF.TEXT:= ST;&#13;&#10;       HURUF2.TEXT:= ST;&#13;&#10;       DMPMemo51.TEXT:= FORMATFLOAT('#,##0.00', SUBTOTAL + GETBULAT(SUBTOTAL));&#13;&#10;       DMPMemo68.TEXT:= ': Rp. ' + DMPMemo51.TEXT;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;       CREATEBILANGAN;&#13;&#10;       QRYProperti.Close;&#13;&#10;       QRYProperti.SQL.Text:= 'SELECT * FROM UNITTAHUN WHERE TAHUN=''' + TahunAnggaran + '''' +&#13;&#10;                       ' AND UnitKd=''' + UnitKd + '''';&#13;&#10;       QRYProperti.Open;&#13;&#10;end." PropData="">
