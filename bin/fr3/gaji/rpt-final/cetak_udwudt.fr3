    <TfrxADOQuery Name="qryGaji" UserName="qryGaji" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;A.NIP, A.NAMA, ISNULL(A.JML_SI,0)JISTRI, A.JNS_KELAMIN,&#13;&#10;ISNULL(A.JML_ANAK,0)JANAK, (ISNULL(JML_SI, 0)+ISNULL(JML_ANAK,0)+1) AS JML_JIWA,&#13;&#10;ISNULL(A.AKTIF_TGL, GETDATE()) AS TGL, B.GOLONGANKD, B.URAIAN AS PANGKAT, C.UNITKD, C.UNITNM,&#13;&#10;A.GAJI_POKOK, A.TUNJ_ISTRI, A.TUNJ_ANAK, A.TUNJ_BERAS,&#13;&#10;A.TUNJ_UMUM, A.TUNJ_UMUM_TAMB, A.TUNJ_JAB_STRUKTUR, A.TUNJ_JAB_FUNGSI,&#13;&#10;A.PEMBULATAN, A.PENERIMA_UDWUDT, A.TGLBYR_UDWUDT&#13;&#10;FROM&#13;&#10;PEGAWAI_GAJI A INNER JOIN TBLGOLONGAN B ON A.GOLONGANKD=B.GOLONGANKD&#13;&#10;INNER JOIN TBLUNIT C ON A.UNITKD=C.UNITKD&#13;&#10;WHERE&#13;&#10;--strWhere AND&#13;&#10;1=1" CommandTimeout="30" PropData="0A506172616D65746572730100"/>
