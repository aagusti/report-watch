<TfrxReport Version="4.0.11" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="\\PROPERINDO2\Epson LQ-2180 ESC/P 2" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40169.7413190741" ReportOptions.Description.Text="" ReportOptions.LastChange="40277.2109441898" ScriptLanguage="PascalScript" ScriptText.Text="VAR //UNITKD:STRING;&#13;&#10;       SQLGOL:STRING;&#13;&#10;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;FUNCTION TGLINDONESIA(T:TDATETIME):STRING;&#13;&#10;VAR H, B, Y:STRING;&#13;&#10;       M: INTEGER;&#13;&#10;BEGIN&#13;&#10;       H:= '0' + INTTOSTR(DAYOF(T));&#13;&#10;       H:= COPY(H, LENGTH(H)-1,2);&#13;&#10;       M:= MONTHOF(T);&#13;&#10;       CASE M OF&#13;&#10;       1: B:='JANUARI';&#13;&#10;       2: B:='FEBRUARI';&#13;&#10;       3: B:='MARET';&#13;&#10;       4: B:='APRIL';&#13;&#10;       5: B:='MEI';&#13;&#10;       6: B:='JUNI';&#13;&#10;       7: B:='JULI';&#13;&#10;       8: B:='AGUSTUS';&#13;&#10;       9: B:='SEPTEMBER';&#13;&#10;       10: B:='OKTOBER';&#13;&#10;       11: B:='NOPEMBER';&#13;&#10;       12: B:='DESEMBER';&#13;&#10;       END;&#13;&#10;       Y:= INTTOSTR(YEAROF(T));&#13;&#10;       RESULT:= H + ' ' + B + ' ' + Y;&#13;&#10;END;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       QRYTEMP.CLOSE;&#13;&#10;       QRYTEMP.SQL.TEXT:= 'SELECT SUM(A.JUMLAH)JML FROM (' + SQLGOL + ') A';&#13;&#10;       QRYTEMP.OPEN;&#13;&#10;       MBULAN.CLEAR;&#13;&#10;       IF (COBULAN.TEXT&#60;&#62;'') THEN MBULAN.TEXT:= ': ' + COBULAN.TEXT + ' ' + COTAHUN.TEXT&#13;&#10;       ELSE MBULAN.TEXT:= ':..................';&#13;&#10;       MREKENING.CLEAR;&#13;&#10;       IF (EREKENING.TEXT&#60;&#62;'') THEN MREKENING.TEXT:= ': ' + UPPERCASE(EREKENING.TEXT)&#13;&#10;       ELSE EREKENING.TEXT:= ':..................';&#13;&#10;       MBANK.CLEAR;&#13;&#10;       IF (EBANK.TEXT&#60;&#62;'') THEN MBANK.TEXT:=  ':' + UPPERCASE(EBANK.TEXT)&#13;&#10;       ELSE MBANK.TEXT:= ':..................';&#13;&#10;       MLOKASI.CLEAR;&#13;&#10;       IF (ELOK.TEXT&#60;&#62;'') THEN MLOKASI.TEXT:= ': ' + UPPERCASE(ELOK.TEXT)&#13;&#10;       ELSE MLOKASI.TEXT:= ':..................';&#13;&#10;       MNOMOR.CLEAR;&#13;&#10;       IF (ENOMOR.TEXT&#60;&#62;'') THEN MNOMOR.TEXT:= UPPERCASE(ENOMOR.TEXT)&#13;&#10;       ELSE MNOMOR.TEXT:= '.................................';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       NAMA1.CLEAR;&#13;&#10;       MNIP1.CLEAR;&#13;&#10;       IF (TTD1.TEXT&#60;&#62;'') THEN&#13;&#10;       BEGIN&#13;&#10;               NAMA1.TEXT:= UPPERCASE(TTD1.TEXT);&#13;&#10;               MNIP1.TEXT:= 'NIP.' + UPPERCASE(NIP1.TEXT);&#13;&#10;       END&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               NAMA1.TEXT:= '                ';&#13;&#10;               MNIP1.TEXT:= 'NIP.            ';&#13;&#10;       END;&#13;&#10;       NAMA2.CLEAR;&#13;&#10;       MNIP2.CLEAR;&#13;&#10;       IF (TTD2.TEXT&#60;&#62;'') THEN&#13;&#10;       BEGIN&#13;&#10;               NAMA2.TEXT:= UPPERCASE(TTD2.TEXT);&#13;&#10;               MNIP2.TEXT:= 'NIP.' + UPPERCASE(NIP2.TEXT);&#13;&#10;       END&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               NAMA2.TEXT:= '                ';&#13;&#10;               MNIP2.TEXT:= 'NIP.            ';&#13;&#10;       END;&#13;&#10;       NAMA3.CLEAR;&#13;&#10;       MNIP3.CLEAR;&#13;&#10;       IF (TTD3.TEXT&#60;&#62;'') THEN&#13;&#10;       BEGIN&#13;&#10;               NAMA3.TEXT:= UPPERCASE(TTD3.TEXT);&#13;&#10;               MNIP3.TEXT:= 'NIP.' + UPPERCASE(NIP3.TEXT);&#13;&#10;       END&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               NAMA3.TEXT:= '                ';&#13;&#10;               MNIP3.TEXT:= 'NIP.            ';&#13;&#10;       END;&#13;&#10;       MLAPOR.CLEAR;&#13;&#10;       MLAPOR.TEXT:= 'TANGERANG, ' + TGLINDONESIA(TANGGAL.DATE);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       TANGGAL.DATE:= DATE;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;VAR BLN, THN, JNS: STRING;&#13;&#10;begin&#13;&#10;       if rbGaji13.Checked then&#13;&#10;            JNS:= '1'&#13;&#10;       else if rbGajiTerus.Checked then&#13;&#10;               JNS:= '6'&#13;&#10;       else&#13;&#10;               JNS:= '0';&#13;&#10;&#13;&#10;       BLN:= '0' + INTTOSTR(COBULAN.ITEMINDEX + 1);&#13;&#10;       BLN:= COPY(BLN, LENGTH(BLN)-1, 2);&#13;&#10;       THN:= COTAHUN.TEXT;&#13;&#10;       QRYGOL.CLOSE;&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '99999', JNS);&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '{BULAN}', BLN);&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '{TAHUN}', THN);&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '{UNITKD}', UNITKD);&#13;&#10;       SQLGOL:= QRYGOL.SQL.TEXT;&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '---//', '');&#13;&#10;       QRYGOL.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;       QRYTAHUN.OPEN;&#13;&#10;       QRYSKPD.CLOSE;&#13;&#10;       QRYSKPD.PARAMS[0].VALUE:=UNITKD;&#13;&#10;       QRYSKPD.OPEN;&#13;&#10;&#13;&#10;end." PropData="">
