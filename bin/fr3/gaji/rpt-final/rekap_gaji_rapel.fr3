<TfrxReport Version="3.22" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="1273" PreviewOptions.MDIChild="True" PreviewOptions.Modal="False" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="40575.6688921412" ReportOptions.Description.Text="" ReportOptions.LastChange="40575.6783271759" ScriptLanguage="PascalScript" ScriptText.Text="VAR //JMLSUSUNAN, KOTOR, POT, BERSIH, JMLPERBARIS: DOUBLE;&#13;&#10;       SQLSTR, TMTAHUN, THNGAJI, TMBULAN, BULANGAJI, BULANNM, KEPALA, T1, N1, T2, N2, T3, N3: STRING;&#13;&#10;       LKS, RAPELBLN: STRING;&#13;&#10;       TGL: TDATETIME;&#13;&#10;&#13;&#10;       BANYAK: INTEGER;&#13;&#10;       MULAI, SAMPAI: STRING;&#13;&#10;       SUMSUSUNAN1, SUMSUSUNAN2, SUMSUSUNAN3, SUMSUSUNAN4: INTEGER;&#13;&#10;       SUMKOTOR1, SUMKOTOR2, SUMKOTOR3, SUMKOTOR4: REAL;&#13;&#10;       SUMPOTONGAN1, SUMPOTONGAN2, SUMPOTONGAN3, SUMPOTONGAN4: REAL;&#13;&#10;       LAINLAIN:ARRAY[1..4] OF REAL;&#13;&#10;&#13;&#10;       RAPELNIP: String;&#13;&#10;&#13;&#10;       IsUseSUB: Boolean&#13;&#10;&#13;&#10;function HasSubUnit(kode_skpd : string): boolean;&#13;&#10;begin&#13;&#10;qrySub.Close;&#13;&#10;  ReplaceStr(qrySub.SQL.Text, '&#60;unitkd&#62;', kode_skpd);&#13;&#10;  qrySub.Open;&#13;&#10;  Result:= (&#60;qrySub.&#34;sub_x&#34;&#62; &#62; 1);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;FUNCTION TGLINDONESIA(T:TDATETIME):STRING;&#13;&#10;VAR H, B, Y:STRING;&#13;&#10;       M: INTEGER;&#13;&#10;BEGIN&#13;&#10;       H:= '0' + INTTOSTR(DAYOF(T));&#13;&#10;       H:= COPY(H,Length(H)-1,2);&#13;&#10;       M:= MONTHOF(T);&#13;&#10;       CASE M OF&#13;&#10;       1: B:='JANUARI';&#13;&#10;       2: B:='FEBRUARI';&#13;&#10;       3: B:='MARET';&#13;&#10;       4: B:='APRIL';&#13;&#10;       5: B:='MEI';&#13;&#10;       6: B:='JUNI';&#13;&#10;       7: B:='JULI';&#13;&#10;       8: B:='AGUSTUS';&#13;&#10;       9: B:='SEPTEMBER';&#13;&#10;       10: B:='OKTOBER';&#13;&#10;       11: B:='NOPEMBER';&#13;&#10;       12: B:='DESEMBER';&#13;&#10;       END;&#13;&#10;       Y:= INTTOSTR(YEAROF(T));&#13;&#10;       RESULT:= H + ' ' + B + ' ' + Y;&#13;&#10;END;&#13;&#10;&#13;&#10;FUNCTION GETBANYAKRAPEL: INTEGER;&#13;&#10;VAR TMTBLN, BLN, TMTTHN, THN, RES: INTEGER;&#13;&#10;BEGIN&#13;&#10;       IF (QRYRAPEL.RECORDCOUNT&#60;1) THEN&#13;&#10;               RES:= 0&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               TMTBLN:= STRTOINT(&#60;QRYRAPEL.&#34;TM_BULAN&#34;&#62;);&#13;&#10;               TMTTHN:= STRTOINT(&#60;QRYRAPEL.&#34;TM_TAHUN&#34;&#62;);&#13;&#10;               BLN:= STRTOINT(&#60;QRYRAPEL.&#34;BULAN&#34;&#62;);&#13;&#10;               THN:= STRTOINT(&#60;QRYRAPEL.&#34;TAHUN&#34;&#62;);&#13;&#10;               IF (THN=TMTTHN) THEN&#13;&#10;               BEGIN&#13;&#10;                   IF (TMTBLN&#62;BLN) THEN RES:=-1&#13;&#10;                   ELSE RES:= BLN-TMTBLN;&#13;&#10;               END&#13;&#10;               ELSE IF (THN&#62;TMTTHN) THEN&#13;&#10;               BEGIN&#13;&#10;                       RES:= (BLN + (12 * (THN-TMTTHN)))-TMTBLN;&#13;&#10;               END&#13;&#10;               ELSE RES:=-1;&#13;&#10;       END;&#13;&#10;       RESULT:= RES+1;&#13;&#10;END;&#13;&#10;&#13;&#10;PROCEDURE GETJMLSUSUNAN(VAR TXT: INTEGER; FLD, GOL, SQLMATCH: STRING; LEFTGOL: INTEGER; SIA: INTEGER=0);&#13;&#10;VAR S: STRING;&#13;&#10;BEGIN&#13;&#10;       IF (QRYRAPEL.RECORDCOUNT&#60;1) THEN&#13;&#10;               TXT:= 0&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               TMP1.CLOSE;&#13;&#10;               S:= 'SELECT ' + SQLMATCH + '(' + FLD + ') AS JML FROM PEGAWAI_GAJI_RAPEL ' +&#13;&#10;                       'WHERE TM_BULAN=''' + &#60;QRYRAPEL.&#34;TM_BULAN&#34;&#62; + ''' AND TM_TAHUN=''' +&#13;&#10;                       &#60;QRYRAPEL.&#34;TM_TAHUN&#34;&#62; + ''' AND BULAN=''' + &#60;QRYRAPEL.&#34;BULAN&#34;&#62; + ''' AND ' +&#13;&#10;                       'TAHUN=''' + &#60;QRYRAPEL.&#34;TAHUN&#34;&#62; + ''' AND JENIS=' + INTTOSTR(&#60;QRYRAPEL.&#34;JENIS&#34;&#62;) +&#13;&#10;                       ' AND LEFT(GOLONGANKD,' + INTTOSTR(LEFTGOL) + ')=''' + GOL + '''';&#13;&#10;               if (IsUseSub) then&#13;&#10;                       S:= S + ' AND UNITKD LIKE ''' + UnitKd + '%'''&#13;&#10;               else&#13;&#10;                       S:= S + ' AND UNITKD=''' + UnitKd + '''';&#13;&#10;               CASE SIA OF&#13;&#10;               1: S:= S + ' AND JML_SI&#62;0';&#13;&#10;               2: S:= S + ' AND JML_ANAK&#62;0';&#13;&#10;               END;&#13;&#10;               IF (RAPELNIP&#60;&#62;'') THEN S:= S + ' AND ((nip like ''' + RAPELNIP + '%'') or (nama like ''' + RAPELNIP + '%''))';&#13;&#10;               TMP1.SQL.TEXT:= S;&#13;&#10;               try&#13;&#10;               TMP1.OPEN;&#13;&#10;               TXT:= &#60;TMP1.&#34;JML&#34;&#62;;&#13;&#10;               except&#13;&#10;                       //showmessage('Di susunan');&#13;&#10;               end;&#13;&#10;       END;&#13;&#10;END;&#13;&#10;&#13;&#10;&#13;&#10;PROCEDURE SETTGLGAJI(VAR TAWAL, TAKHIR: STRING);&#13;&#10;BEGIN&#13;&#10;       IF (QRYRAPEL.RECORDCOUNT&#60;1) THEN&#13;&#10;       BEGIN&#13;&#10;               TAWAL:= '1900/01/01';&#13;&#10;               TAKHIR:= '1900/01/01';&#13;&#10;       END&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               TAWAL:= &#60;QRYRAPEL.&#34;TM_TAHUN&#34;&#62; + '/' + &#60;QRYRAPEL.&#34;TM_BULAN&#34;&#62; + '/01';&#13;&#10;               TAKHIR:= &#60;QRYRAPEL.&#34;TAHUN&#34;&#62; + '/' + &#60;QRYRAPEL.&#34;BULAN&#34;&#62; + '/10';&#13;&#10;       END;&#13;&#10;END;&#13;&#10;&#13;&#10;&#13;&#10;PROCEDURE GETTOTALRAPEL(VAR TOT: VARIANT; FLD, GOL, SQLMATH: STRING; LEFTGOL: INTEGER; KOL: INTEGER=1);&#13;&#10;VAR GJ, GJHUT, GJLBH, LAIN, RAP: VARIANT;&#13;&#10;       S: STRING;&#13;&#10;BEGIN&#13;&#10;       IF (FLD='LAIN') THEN&#13;&#10;       BEGIN&#13;&#10;               //LAINLAIN[KOL]:= LAINLAIN[KOL] * BANYAK;&#13;&#10;               TOT:= LAINLAIN[KOL];// * BANYAK;&#13;&#10;               EXIT;&#13;&#10;       END;&#13;&#10;       TMP2.CLOSE;&#13;&#10;       S:= 'SELECT A.UNITKD,' +&#13;&#10;               '(SUM(CASE WHEN A.' + FLD + ' - ISNULL(B.' + FLD + ',0)&#60;0 THEN 0 ELSE A.'+FLD+' - ISNULL(B.' + FLD + ',0) END)*' + INTTOSTR(BANYAK) + ') TOTAL, ' +&#13;&#10;               '(SUM(CASE WHEN A.' + FLD + ' - ISNULL(B.' + FLD + ',0)&#60;0 THEN ABS(A.' + FLD + ' - ISNULL(B.' + FLD + ',0)) ELSE 0 END)*' + INTTOSTR(BANYAK) + ') LAIN ' +&#13;&#10;               ' FROM PEGAWAI_GAJI_RAPEL A LEFT OUTER JOIN ' +&#13;&#10;               '(SELECT * FROM PEGAWAI_GAJI WHERE ' +&#13;&#10;               'JENIS=0 AND BULAN=''' + &#60;QRYRAPEL.&#34;BULAN&#34;&#62; + '''' +&#13;&#10;               ' AND TAHUN=''' + &#60;QRYRAPEL.&#34;TAHUN&#34;&#62; + '''' +')B ON (A.NIP=B.NIP AND A.BULAN=B.BULAN AND A.TAHUN=B.TAHUN) ' +&#13;&#10;               ' WHERE (A.TM_BULAN=''' + &#60;QRYRAPEL.&#34;TM_BULAN&#34;&#62; + '''' +&#13;&#10;               ' AND A.TM_TAHUN=''' + &#60;QRYRAPEL.&#34;TM_TAHUN&#34;&#62; + '''' +&#13;&#10;               ' AND A.BULAN=''' + &#60;QRYRAPEL.&#34;BULAN&#34;&#62; + '''' +&#13;&#10;               ' AND A.TAHUN=''' + &#60;QRYRAPEL.&#34;TAHUN&#34;&#62; + '''' +&#13;&#10;               ' AND A.JENIS=' + INTTOSTR(&#60;QRYRAPEL.&#34;JENIS&#34;&#62;) + ')' +&#13;&#10;               //' OR (B.BULAN=''' + &#60;QRYRAPEL.&#34;BULAN&#34;&#62; + '''' +&#13;&#10;               //' AND B.TAHUN=''' + &#60;QRYRAPEL.&#34;TAHUN&#34;&#62; + '''' +&#13;&#10;               //' OR (B.JENIS=0) ' +&#13;&#10;               ' AND LEFT(A.GOLONGANKD,' + INTTOSTR(LEFTGOL) + ') = ''' + GOL + '''';&#13;&#10;       if (IsUseSub) then&#13;&#10;                       S:= S + ' AND A.UNITKD LIKE ''' + UnitKd + '%'''&#13;&#10;               else&#13;&#10;                       S:= S + ' AND A.UNITKD=''' + UnitKd + '''';&#13;&#10;               //' AND A.UNITKD=''' + UNITKD + '''';&#13;&#10;       IF (RAPELNIP&#60;&#62;'') THEN S:= S + ' AND ((A.nip like ''' + RAPELNIP + '%'') or (A.nama like ''' + RAPELNIP + '%''))';&#13;&#10;       S:= S + ' GROUP BY A.UNITKD';&#13;&#10;       //SHOWMESSAGE(S);&#13;&#10;       TMP2.SQL.TEXT:= S;&#13;&#10;       TMP2.OPEN;&#13;&#10;       TOT:= &#60;TMP2.&#34;TOTAL&#34;&#62;;&#13;&#10;       LAINLAIN[KOL]:= LAINLAIN[KOL] + &#60;TMP2.&#34;LAIN&#34;&#62;;&#13;&#10;&#13;&#10;END;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;VAR&#13;&#10;       G1, G2, G3, G4, GTOT: REAL;&#13;&#10;       N, GJ, RAPEL: REAL;&#13;&#10;       J, J1, J2, J3, J4, JTOT: INTEGER;&#13;&#10;begin&#13;&#10;       N:= 0; GJ:= 0; RAPEL:= 0;&#13;&#10;       GOL1.TEXT:='';&#13;&#10;       GOL2.TEXT:='';&#13;&#10;       GOL3.TEXT:='';&#13;&#10;       GOL4.TEXT:='';&#13;&#10;       TOTALGOL.TEXT:='';&#13;&#10;       IF (&#60;QRYREKAP.&#34;ISSUMMARY&#34;&#62;=0) THEN&#13;&#10;       BEGIN&#13;&#10;               IF (&#60;QRYREKAP.&#34;ISRINCIAN&#34;&#62;=0)  THEN&#13;&#10;               BEGIN&#13;&#10;                       GETJMLSUSUNAN(J, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'I/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 2, &#60;QRYREKAP.&#34;ISISTRI&#34;&#62;);&#13;&#10;                       GOL1.TEXT:= INTTOSTR(J);&#13;&#10;                       J1:= J;&#13;&#10;                       SUMSUSUNAN1:= SUMSUSUNAN1 + J1;&#13;&#10;                       GETJMLSUSUNAN(J, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'II/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 3, &#60;QRYREKAP.&#34;ISISTRI&#34;&#62;);&#13;&#10;                       GOL2.TEXT:= INTTOSTR(J);&#13;&#10;                       J2:= J;&#13;&#10;                       SUMSUSUNAN2:= SUMSUSUNAN2 + J2;&#13;&#10;                       GETJMLSUSUNAN(J, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'III/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 4, &#60;QRYREKAP.&#34;ISISTRI&#34;&#62;);&#13;&#10;                       GOL3.TEXT:= INTTOSTR(J);&#13;&#10;                       J3:= J;&#13;&#10;                       SUMSUSUNAN3:= SUMSUSUNAN3 + J3;&#13;&#10;                       GETJMLSUSUNAN(J, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'IV/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 3, &#60;QRYREKAP.&#34;ISISTRI&#34;&#62;);&#13;&#10;                       GOL4.TEXT:= INTTOSTR(J);&#13;&#10;                       J4:= J;&#13;&#10;                       SUMSUSUNAN4:= SUMSUSUNAN4 + J4;&#13;&#10;                       TOTALGOL.TEXT:= INTTOSTR(J1+J2+J3+J4);&#13;&#10;               END&#13;&#10;               ELSE&#13;&#10;               BEGIN&#13;&#10;                       GETTOTALRAPEL(G1, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'I/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 2, 1);&#13;&#10;                       GOL1.TEXT:= FORMATFLOAT('#,##0', G1);&#13;&#10;                       IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=1) THEN&#13;&#10;                               SUMKOTOR1:= SUMKOTOR1 + G1&#13;&#10;                       ELSE IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=-1) THEN&#13;&#10;                               SUMPOTONGAN1:= SUMPOTONGAN1 + G1;&#13;&#10;&#13;&#10;                       GETTOTALRAPEL(G2, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'II/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 3, 2);&#13;&#10;&#13;&#10;                       GOL2.TEXT:=  FORMATFLOAT('#,##0', G2);&#13;&#10;                       IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=1) THEN&#13;&#10;                               SUMKOTOR2:= SUMKOTOR2 + G2&#13;&#10;                       ELSE IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=-1) THEN&#13;&#10;                               SUMPOTONGAN2:= SUMPOTONGAN2 + G2;&#13;&#10;&#13;&#10;                       GETTOTALRAPEL(G3, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'III/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 4, 3);&#13;&#10;&#13;&#10;                       GOL3.TEXT:= FORMATFLOAT('#,##0', G3);&#13;&#10;                       IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=1) THEN&#13;&#10;                               SUMKOTOR3:= SUMKOTOR3 + G3&#13;&#10;                       ELSE IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=-1) THEN&#13;&#10;                               SUMPOTONGAN3:= SUMPOTONGAN3 + G3;&#13;&#10;&#13;&#10;                       GETTOTALRAPEL(G4, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'IV/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 3, 4);&#13;&#10;&#13;&#10;                       GOL4.TEXT:= FORMATFLOAT('#,##0', G4);&#13;&#10;                       IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=1) THEN&#13;&#10;                               SUMKOTOR4:= SUMKOTOR4 + G4&#13;&#10;                       ELSE IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=-1) THEN&#13;&#10;                               SUMPOTONGAN4:= SUMPOTONGAN4 + G4;&#13;&#10;                       TOTALGOL.TEXT:= FORMATFLOAT('#,##0', G1 + G2 + G3 + G4);&#13;&#10;               END;&#13;&#10;       END&#13;&#10;       ELSE IF (&#60;QRYREKAP.&#34;ISSUMMARY&#34;&#62;=1) THEN&#13;&#10;       BEGIN&#13;&#10;               IF (&#60;QRYREKAP.&#34;ISRINCIAN&#34;&#62;=0)  THEN&#13;&#10;               BEGIN&#13;&#10;                       GOL1.TEXT:= INTTOSTR(SUMSUSUNAN1);&#13;&#10;                       GOL2.TEXT:= INTTOSTR(SUMSUSUNAN2);&#13;&#10;                       GOL3.TEXT:= INTTOSTR(SUMSUSUNAN3);&#13;&#10;                       GOL4.TEXT:= INTTOSTR(SUMSUSUNAN4);&#13;&#10;                       TOTALGOL.TEXT:= INTTOSTR(SUMSUSUNAN1+SUMSUSUNAN2+SUMSUSUNAN3+SUMSUSUNAN4);&#13;&#10;               END&#13;&#10;               ELSE&#13;&#10;               BEGIN&#13;&#10;                       CASE &#60;QRYREKAP.&#34;HITUNGKD&#34;&#62; OF&#13;&#10;                       1 :&#13;&#10;                               BEGIN&#13;&#10;                                       GOL1.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR1);&#13;&#10;                                       GOL2.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR2);&#13;&#10;                                       GOL3.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR3);&#13;&#10;                                       GOL4.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR4);&#13;&#10;                                       TOTALGOL.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR1+SUMKOTOR2+SUMKOTOR3+SUMKOTOR4);&#13;&#10;                               END;&#13;&#10;                       2 :&#13;&#10;                               BEGIN&#13;&#10;                                       GOL1.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR1-SUMPOTONGAN1);&#13;&#10;                                       GOL2.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR2-SUMPOTONGAN2);&#13;&#10;                                       GOL3.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR3-SUMPOTONGAN3);&#13;&#10;                                       GOL4.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR4-SUMPOTONGAN4);&#13;&#10;                                       TOTALGOL.TEXT:= FORMATFLOAT('#,##0', (SUMKOTOR1-SUMPOTONGAN1)+(SUMKOTOR2-SUMPOTONGAN2)+(SUMKOTOR3-SUMPOTONGAN3)+(SUMKOTOR4-SUMPOTONGAN4));&#13;&#10;                               END;&#13;&#10;                       -1 :&#13;&#10;                               BEGIN&#13;&#10;                                       GOL1.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN1);&#13;&#10;                                       GOL2.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN2);&#13;&#10;                                       GOL3.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN3);&#13;&#10;                                       GOL4.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN4);&#13;&#10;                                       TOTALGOL.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN1+SUMPOTONGAN2+SUMPOTONGAN3+SUMPOTONGAN4);&#13;&#10;                               END;&#13;&#10;                       END;&#13;&#10;               END;&#13;&#10;       END;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       MEMUNIT.TEXT:= ': ' + UNITKD;&#13;&#10;       MEMBULAN.TEXT:= ': ' + RAPELBLN;&#13;&#10;       MLOKASI.CLEAR;&#13;&#10;       IF (LKS&#60;&#62;'') then&#13;&#10;               MLOKASI.TEXT:= 'LOKASI : ' + UPPERCASE(LKS)&#13;&#10;       else&#13;&#10;               MLOKASI.TEXT:= 'LOKASI :           ';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;VAR    JNS, STRWHERE: STRING;&#13;&#10;       D1, D2: STRING;&#13;&#10;begin&#13;&#10;       RapelNIP:= eNIP.Text;&#13;&#10;       LKS:= ELOK.TEXT;&#13;&#10;       BULANNM:= COBULAN.TEXT;&#13;&#10;       if (COBULAN.TEXT=COBULAN2.TEXT)AND(COTAHUN.TEXT=COTAHUN2.TEXT) THEN&#13;&#10;               RAPELBLN:= COBULAN.TEXT + ' ' + COTAHUN.TEXT&#13;&#10;       ELSE&#13;&#10;               RAPELBLN:= COBULAN.TEXT + ' ' + COTAHUN.TEXT + ' s/d ' + COBULAN2.TEXT + ' ' + COTAHUN2.TEXT;&#13;&#10;       TGL:= TANGGAL.DATE;&#13;&#10;       //KEPALA:= NMKPL.TEXT;&#13;&#10;       //T1:= TTD1.TEXT;&#13;&#10;       //T2:= TTD2.TEXT;&#13;&#10;       //T3:= TTD3.TEXT;&#13;&#10;       //N1:= NIP1.TEXT;&#13;&#10;       //N2:= NIP2.TEXT;&#13;&#10;       //N3:= NIP3.TEXT;&#13;&#10;       IF (COBULAN.TEXT)='' THEN&#13;&#10;               TMBULAN:= ''&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               TMBULAN:= '0' + INTTOSTR(COBULAN.ITEMINDEX+1);&#13;&#10;               TMBULAN:= COPY(TMBULAN, Length(TMBULAN)-1, 2);&#13;&#10;       END;&#13;&#10;&#13;&#10;       IF (COBULAN2.TEXT)='' THEN&#13;&#10;               BULANGAJI:= ''&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               BULANGAJI:= '0' + INTTOSTR(COBULAN2.ITEMINDEX+1);&#13;&#10;               BULANGAJI:= COPY(BULANGAJI, Length(BULANGAJI)-1, 2);&#13;&#10;       END;&#13;&#10;       IF (RBKURANG.CHECKED) THEN JNS:= '15'&#13;&#10;       ELSE IF (RBBERAS.CHECKED) THEN JNS:= '14'&#13;&#10;       ELSE IF (RBUMUM.CHECKED) THEN JNS:= '13'&#13;&#10;       ELSE IF (RBFUNGSI.CHECKED) THEN JNS:= '12'&#13;&#10;       ELSE IF (RBSTRUK.CHECKED) THEN JNS:= '11'&#13;&#10;       ELSE JNS:= '10';&#13;&#10;       THNGAJI:= COTAHUN2.TEXT;&#13;&#10;       TMTAHUN:= COTAHUN.TEXT;&#13;&#10;       D1:= TMBULAN + '/01/' + TMTAHUN;&#13;&#10;       D2:= BULANGAJI + '/15/' + THNGAJI;&#13;&#10;       STRWHERE := ' TM_BULAN=''' + TMBULAN + ''' AND TM_TAHUN=''' + TMTAHUN + '''' +&#13;&#10;               ' AND BULAN=''' + BULANGAJI + ''' AND TAHUN=''' + THNGAJI + '''' +&#13;&#10;               ' AND JENIS=' + JNS;&#13;&#10;       QRYRAPEL.CLOSE;&#13;&#10;       REPLACESTR(QRYRAPEL.SQL.TEXT, '1=1', STRWHERE);&#13;&#10;       QRYRAPEL.OPEN;&#13;&#10;       BANYAK:= GETBANYAKRAPEL;&#13;&#10;       SETTGLGAJI(MULAI, SAMPAI);&#13;&#10;       IsUseSUB:= (cbIncludeSub.Visible and cbIncludeSub.Checked);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       TGLLAP.TEXT:= 'TANGERANG, ' + TGLINDONESIA(TGL);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       TANGGAL.DATE:= DATE;&#13;&#10;         cbIncludeSub.Visible:= HasSubUnit(UnitKd);&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;       LAINLAIN[1]:= 0;&#13;&#10;       LAINLAIN[2]:= 0;&#13;&#10;       LAINLAIN[3]:= 0;&#13;&#10;       LAINLAIN[4]:= 0;&#13;&#10;       SUMSUSUNAN1:= 0;&#13;&#10;       SUMSUSUNAN2:= 0;&#13;&#10;       SUMSUSUNAN3:= 0;&#13;&#10;       SUMSUSUNAN4:= 0;&#13;&#10;       SUMKOTOR1:= 0;&#13;&#10;       SUMKOTOR2:= 0;&#13;&#10;       SUMKOTOR3:= 0;&#13;&#10;       SUMKOTOR4:= 0;&#13;&#10;       SUMPOTONGAN1:= 0;&#13;&#10;       SUMPOTONGAN2:= 0;&#13;&#10;       SUMPOTONGAN3:= 0;&#13;&#10;       SUMPOTONGAN4:= 0;&#13;&#10;       QRYTAHUN.OPEN;&#13;&#10;       QRYTMTAHUN.OPEN;&#13;&#1