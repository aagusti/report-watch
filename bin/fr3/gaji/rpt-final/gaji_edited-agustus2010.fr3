<TfrxReport Version="4.0.11" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40169.7413190741" ReportOptions.Description.Text="" ReportOptions.LastChange="40402.4401667361" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  Bulan : array [0..12] of string;&#13;&#10;  Tahun, BulanKe : integer;&#13;&#10;  NamaBulan, txtGol  : String;&#13;&#10;  zeroVal   : Double;&#13;&#10;  strWHERE:string;&#13;&#10;  stemp:string;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;function HasSubUnit(kode_skpd : string): boolean;&#13;&#10;begin&#13;&#10;{  qrySubUnit.Close;&#13;&#10;  qrySubUnit.ParamByName('kode_skpd').Value := kode_skpd;&#13;&#10;  qrySubUnit.Open;&#13;&#10;&#13;&#10;  Result := &#60;qrySubUnit.&#34;X&#34;&#62; &#62; 1;}&#13;&#10;&#13;&#10;  qrySub.Close;&#13;&#10;  ReplaceStr(qrySub.SQL.Text, '&#60;unitkd&#62;', unitkd);&#13;&#10;  qrySub.Open;&#13;&#10;  Result:= (&#60;qrySub.&#34;sub_x&#34;&#62; &#62; 0);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i : integer;&#13;&#10;begin&#13;&#10;  cbTahun.Items.Clear;&#13;&#10;  while not qryTA.Eof do&#13;&#10;  begin&#13;&#10;    cbTahun.Items.Add( &#60;qryTA.&#34;Tahun&#34;&#62; );&#13;&#10;    qryTA.Next;&#13;&#10;  end;&#13;&#10;  cbTahun.ItemIndex := 0;&#13;&#10;&#13;&#10;  cbBulan.Items.Clear;&#13;&#10;  for i := 1 to 12 do&#13;&#10;    cbBulan.Items.Add( Bulan[i] );&#13;&#10;  cbBulan.ItemIndex := MonthOf(Date) - 1;&#13;&#10;&#13;&#10;  dtTanggal.Date := Date;&#13;&#10;  qrySKPD.Close;&#13;&#10;  qrySKPD.Params[0].Value:=UnitKd;&#13;&#10;  qrySKPD.Open;&#13;&#10;&#13;&#10;{  if HasSubUnit(&#60;qrySKPD.&#34;KODE_SKPD&#34;&#62;) then&#13;&#10;    cbIncludeSub.Visible := True&#13;&#10;  else&#13;&#10;    cbIncludeSub.Visible := False;}&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Tahun := strtoint( cbTahun.Text );&#13;&#10;&#13;&#10;  qryGaji.Close;&#13;&#10;  strWhere:='(g.Tahun='''+cbTahun.Text +''')';&#13;&#10;&#13;&#10;  stemp:='0'+intToStr(cbBulan.ItemIndex + 1);&#13;&#10;  stemp:=copy(stemp,length(stemp)-1,2);&#13;&#10;  strWhere:=strWhere+'AND (g.bulan='''+ stemp+''')';&#13;&#10;&#13;&#10;  strWhere:=strWhere+'AND (g.UnitKd='''+UnitKd+''')';&#13;&#10;&#13;&#10;  if edNamaNIP.Text &#60;&#62; '' then&#13;&#10;  begin&#13;&#10;    strWhere:=strWhere + ' AND ((g.nip like ''' + edNamaNIP.Text + '%'') or (g.nama like ''' + edNamaNIP.Text + '%''))';&#13;&#10;  end;&#13;&#10;&#13;&#10;  if rbGaji13.Checked then&#13;&#10;  begin&#13;&#10;    BulanKe := 0;&#13;&#10;    strWhere:=strWhere+' AND (g.jenis=1)';&#13;&#10;    NamaBulan := 'Gaji ke-13 (' + Bulan[cbBulan.ItemIndex + 1] + ')';&#13;&#10;  end&#13;&#10;  else if rbGajiTerus.Checked then&#13;&#10;  begin&#13;&#10;    strWhere:=strWhere+' AND (g.jenis=6)';&#13;&#10;    BulanKe := cbBulan.ItemIndex + 1;&#13;&#10;    NamaBulan := Bulan[cbBulan.ItemIndex + 1];&#13;&#10;  end&#13;&#10;  else&#13;&#10;  begin&#13;&#10;    strWhere:=strWhere+' AND (g.jenis=0)  and (not g.aktif_kd in(3,4,5,6))';&#13;&#10;    BulanKe := cbBulan.ItemIndex + 1;&#13;&#10;    NamaBulan := Bulan[cbBulan.ItemIndex + 1];&#13;&#10;  end;&#13;&#10;  ReplaceStr(qryGaji.SQL.Text, 'strWhere', strWhere);&#13;&#10;  //ShowMessage(qryGaji.SQL.Text);&#13;&#10;  qryGaji.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  qryAdd.Close;&#13;&#10;  qryAdd.SQL.Text:= 'SELECT PEGAWAI.NIP, PEGAWAI.TMT_PEGAWAI, ' +&#13;&#10;               'PEGAWAI.MASAKERJA, tblGolongan.URAIAN, PEGAWAI.STS_PEGAWAIKD ' +&#13;&#10;               'FROM PEGAWAI INNER JOIN '+&#13;&#10;               'tblGolongan ON PEGAWAI.GOLONGANKD = tblGolongan.GOLONGANKD ' +&#13;&#10;               'WHERE PEGAWAI.NIP=''' + &#60;qryGaji.&#34;NIP&#34;&#62; + '''';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ColumnHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if (rbGaji13.Checked) then&#13;&#10;       ReplaceStr(POT_1.Text, '&#60;.PERSEN&#62;', '')&#13;&#10;  else if (rbGajiTerus.Checked) then&#13;&#10;       ReplaceStr(POT_1.Text, '&#60;.PERSEN&#62;', '.2%')&#13;&#10;  else&#13;&#10;       ReplaceStr(POT_1.Text, '&#60;.PERSEN&#62;', '.10%');&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  Bulan[00] := '';&#13;&#10;  Bulan[01] := 'Januari';&#13;&#10;  Bulan[02] := 'Februari';&#13;&#10;  Bulan[03] := 'Maret';&#13;&#10;  Bulan[04] := 'April';&#13;&#10;  Bulan[05] := 'Mei';&#13;&#10;  Bulan[06] := 'Juni';&#13;&#10;  Bulan[07] := 'Juli';&#13;&#10;  Bulan[08] := 'Agustus';&#13;&#10;  Bulan[09] := 'September';&#13;&#10;  Bulan[10] := 'Oktober';&#13;&#10;  Bulan[11] := 'November';&#13;&#10;  Bulan[12] := 'Desember';&#13;&#10;&#13;&#10;  zeroVal := 0;&#13;&#10;&#13;&#10;end." PropData="">
