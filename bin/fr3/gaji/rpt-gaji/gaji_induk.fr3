  when left(g.golongankd,1)='I' then 4 end) asc, g.tunj_jab_struktur desc, g.tunj_jab_fungsi desc, g.tunj_umum desc, g.gaji_pokok desc,&#13;&#10;g.golongankd desc, g.MASAKERJA DESC, g.STS_PEGAWAIKD DESC, g.TMT_PEGAWAI ASC" CommandTimeout="30" PropData="0A506172616D65746572730100"/>
