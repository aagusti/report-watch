<TfrxReport Version="3.22" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="1273" PreviewOptions.MDIChild="True" PreviewOptions.Modal="False" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="40578.4822126273" ReportOptions.Description.Text="" ReportOptions.LastChange="40578.4847634491" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  Bulan : array [0..12] of string;&#13;&#10;  Tahun, BulanKe : integer;&#13;&#10;  NamaBulan: String;&#13;&#10;  zeroVal : Double;&#13;&#10;  Pilihan:Integer;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetKodeBulan(Index:Integer):String;&#13;&#10;var R:String;&#13;&#10;begin&#13;&#10;       if (Index&#60;0) then&#13;&#10;               R:=''&#13;&#10;       else if (Index+1&#62;=10) then&#13;&#10;               R:= IntToStr(Index+1)&#13;&#10;       else&#13;&#10;               R:= '0' + IntToStr(Index+1);&#13;&#10;       Result:= R;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i : integer;&#13;&#10;begin&#13;&#10;  cboBulan.Items.Clear;&#13;&#10;  for i := 1 to 12 do&#13;&#10;   cboBulan.Items.Add( Bulan[i] );&#13;&#10;  cboBulan.ItemIndex := MonthOf(Date) - 1;&#13;&#10;  cboTahun.ItemIndex := YearOf(Date);&#13;&#10;  dtTanggal.Date := Date;&#13;&#10;  qryTahun.Close;&#13;&#10;  qryTahun.Open;&#13;&#10;  cboTahun.Items.Clear;&#13;&#10;  if (qryTahun.RecordCount&#62;0) then&#13;&#10;       begin&#13;&#10;               qryTahun.First;&#13;&#10;               while not (qryTahun.Eof) do&#13;&#10;                       begin&#13;&#10;                               cboTahun.Items.Add(qryTahun.FieldByName('Tahun').AsString);&#13;&#10;                               qryTahun.Next;&#13;&#10;                       end;&#13;&#10;               cboTahun.ItemIndex:= 0;&#13;&#10;       end;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;var S, B:String;&#13;&#10;begin&#13;&#10;       B:= GetKodeBulan(cboBulan.ItemIndex);&#13;&#10;       if (B='') then exit;&#13;&#10;       if (cboTahun.Text='') then exit;&#13;&#10;&#13;&#10;       S:= ' ';&#13;&#10;       S:= S + ' AND(PEGAWAI_gaji.Bulan=''' + B + ''')AND(PEGAWAI_gaji.Tahun=''' + cboTahun.Text + ''') ';&#13;&#10;&#13;&#10;       qryGaji.Close;&#13;&#10;       if (eNIp.Text&#60;&#62;'') then&#13;&#10;               S:= S + ' and (PEGAWAI_gaji.nip like ''%' + eNip.Text + '%'' or PEGAWAI_gaji.nama like ''%' + eNip.Text + '%'') ';&#13;&#10;&#13;&#10;       ReplaceStr(qryGaji.SQL.Text, 'and 1=1', S);&#13;&#10;       qryGaji.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10; Bulan[00] := '';&#13;&#10; Bulan[01] := 'Januari';&#13;&#10; Bulan[02] := 'Februari';&#13;&#10; Bulan[03] := 'Maret';&#13;&#10; Bulan[04] := 'April';&#13;&#10; Bulan[05] := 'Mei';&#13;&#10; Bulan[06] := 'Juni';&#13;&#10; Bulan[07] := 'Juli';&#13;&#10; Bulan[08] := 'Agustus';&#13;&#10; Bulan[09] := 'September';&#13;&#10; Bulan[10] := 'Oktober';&#13;&#10; Bulan[11] := 'November';&#13;&#10; Bulan[12] := 'Desember';&#13;&#10;&#13;&#10; zeroVal := 0;&#13;&#10;&#13;&#10; Pilihan:=0;&#13;&#10;end." PropData="08446174617365747301010C2800000020446174615365743D2271727947616A692220446174615365744E616D653D2271727947616A692200010C2A00000020446174615365743D22717279546168756E2220446174615365744E616D653D22717279546168756E220000095661726961626C65730100055374796C650100">
