    <TfrxADOQuery Name="qryGaji" UserName="qryGaji" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT     PEGAWAI_GAJI.TAHUN, PEGAWAI_GAJI.BULAN, PEGAWAI_GAJI.JENIS, PEGAWAI_GAJI.NIP,&#13;&#10;PEGAWAI_GAJI.NAMA, tblGolongan.GOLONGANKD, tblGolongan.URAIAN, PEGAWAI_GAJI.GAJI_POKOK,&#13;&#10;PEGAWAI_GAJI.TUNJ_ISTRI, PEGAWAI_GAJI.TUNJ_ANAK, PEGAWAI_GAJI.TUNJ_BERAS, PEGAWAI_GAJI.TUNJ_UMUM,&#13;&#10;PEGAWAI_GAJI.TUNJ_UMUM_TAMB, PEGAWAI_GAJI.PPH, PEGAWAI_GAJI.TUNJ_JAB_FUNGSI,&#13;&#10;PEGAWAI_GAJI.TUNJ_JAB_STRUKTUR, PEGAWAI_GAJI.PEMBULATAN, PEGAWAI_GAJI.POT_PANGAN, PEGAWAI_GAJI.POT_IWP,&#13;&#10;PEGAWAI_GAJI.POT_TAPERUM, PEGAWAI_GAJI.POT_SEWA_RUMAH, PEGAWAI_GAJI.POT_GAJI_LEBIH,&#13;&#10;PEGAWAI_GAJI.POT_HUTANG,&#13;&#10;PEGAWAI_GAJI.GAJI_POKOK + PEGAWAI_GAJI.TUNJ_ISTRI + PEGAWAI_GAJI.TUNJ_ANAK + PEGAWAI_GAJI.TUNJ_BERAS + PEGAWAI_GAJI.TUNJ_UMUM&#13;&#10;+ PEGAWAI_GAJI.TUNJ_UMUM_TAMB + PEGAWAI_GAJI.PPH + PEGAWAI_GAJI.TUNJ_JAB_FUNGSI + PEGAWAI_GAJI.TUNJ_JAB_STRUKTUR&#13;&#10;+ PEGAWAI_GAJI.PEMBULATAN AS BRUTO,&#13;&#10;PEGAWAI_GAJI.POT_PANGAN + PEGAWAI_GAJI.POT_IWP + PEGAWAI_GAJI.POT_TAPERUM + PEGAWAI_GAJI.POT_SEWA_RUMAH + PEGAWAI_GAJI.POT_GAJI_LEBIH&#13;&#10;+ PEGAWAI_GAJI.POT_HUTANG + PEGAWAI_GAJI.PPH AS POTONGAN, tblBULAN.BULANNM, tblBULAN.NMLENGKAP&#13;&#10;FROM PEGAWAI_GAJI INNER JOIN&#13;&#10;tblGolongan ON PEGAWAI_GAJI.GOLONGANKD = tblGolongan.GOLONGANKD LEFT OUTER JOIN&#13;&#10;tblBULAN ON PEGAWAI_GAJI.BULAN = tblBULAN.BULAN&#13;&#10;WHERE     (PEGAWAI_GAJI.JENIS = 0) and 1=1&#13;&#10;ORDER BY PEGAWAI_gaji.TAHUN DESC, PEGAWAI_gaji.BULAN DESC, PEGAWAI_gaji.NIP ASC" CommandTimeout="30" PropData="0A506172616D65746572730100"/>
