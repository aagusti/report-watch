    <TfrxADOQuery Name="qryGaji" UserName="qryGaji" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10;  g.bulan,&#13;&#10;  g.tahun,&#13;&#10;  g.jenis,&#13;&#10;  left(g.golongankd,len(g.golongankd)-2) as gol,&#13;&#10;  g.golongankd,&#13;&#10;  gg.uraian ,&#13;&#10;  Count(*) as jml_peg,&#13;&#10;  sum(g.jml_si) as jml_si,&#13;&#10;  sum(g.jml_anak) as jml_anak,&#13;&#10;  sum(g.gaji_pokok) as gaji_pokok,&#13;&#10;  sum(g.tunj_istri) as tunj_istri,&#13;&#10;  sum(g.tunj_anak) as tunj_anak,&#13;&#10;  sum(g.tunj_beras) as tunj_beras,&#13;&#10;  sum(g.tunj_kerja) as tunj_kerja,&#13;&#10;  sum(g.tunj_jab_struktur) as tunj_jab_struktur,&#13;&#10;  sum(g.tunj_jab_fungsi) as tunj_jab_fungsi,&#13;&#10;  sum(g.tunj_umum) as tunj_umum,&#13;&#10;  sum(g.tunj_umum_tamb) as tunj_umum_tamb,&#13;&#10;  sum(g.tunj_otsus) as tunj_otsus,&#13;&#10;  sum(g.tunj_dt) as tunj_dt,&#13;&#10;  sum(g.tunj_askes) as tunj_askes,&#13;&#10;  sum(g.tunj_penghasilan) as tunj_penghasilan,&#13;&#10;  sum(g.pph) as pph,&#13;&#10;  sum(g.pot_iwp) as pot_iwp,&#13;&#10;  sum(g.pot_sewa_rumah) as pot_sewa_rumah,&#13;&#10;  sum(g.pot_taperum) as pot_taperum,&#13;&#10;  sum(g.pot_pangan) as pot_pangan,&#13;&#10;  sum(g.pot_korpri) as pot_korpri,&#13;&#10;  sum(g.pot_gaji_lebih) as pot_gaji_lebih,&#13;&#10;  sum(g.pot_hutang) as pot_hutang,&#13;&#10;  sum(g.pembulatan) as  pembulatan&#13;&#10;from PEGAWAI_GAJI g inner join tblunit u on g.unitkd=u.unitkd&#13;&#10;left outer join tblgolongan gg on g.golongankd=gg.golongankd&#13;&#10;where 1=1 --strWhere&#13;&#10;and g.gaji_pokok&#62;0&#13;&#10;group by   g.bulan,&#13;&#10;  g.tahun,&#13;&#10;  g.jenis,&#13;&#10;  left(g.golongankd,len(g.golongankd)-2),&#13;&#10;  g.golongankd,&#13;&#10;  gg.uraian&#13;&#10;&#13;&#10;order by&#13;&#10;  left(g.golongankd,len(g.golongankd)-2),&#13;&#10;  g.golongankd" CommandTimeout="30" PropData="0A506172616D65746572730100"/>
