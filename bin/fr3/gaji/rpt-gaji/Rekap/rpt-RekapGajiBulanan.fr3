    <TfrxADOQuery Name="qryRekap" UserName="qryRekap" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT SORDER = 1, ISRINCIAN = 0, SJENIS = '1. SUSUNAN KELUARGA : - PEGAWAI', TYPEDATA = 'INT', ISSUMMARY = 0, HITUNGKD = 0, SFIELD = 'NIP', SMATCH = 'COUNT', ISISTRI = 0 FROM PEGAWAI&#13;&#10;UNION&#13;&#10;SELECT SORDER = 2, ISRINCIAN = 0, SJENIS = '                      - SUAMI / ISTRI', TYPEDATA = 'INT', ISSUMMARY = 0, HITUNGKD = 0, SFIELD = 'NIP', SMATCH = 'COUNT', ISISTRI = 1 FROM PEGAWAI&#13;&#10;UNION&#13;&#10;SELECT SORDER = 3, ISRINCIAN = 0, SJENIS = '                      - ANAK', TYPEDATA = 'INT', ISSUMMARY = 0, HITUNGKD = 0, SFIELD = 'JML_ANAK', SMATCH = 'SUM', ISISTRI = 0 FROM PEGAWAI&#13;&#10;UNION&#13;&#10;SELECT SORDER = 4, ISRINCIAN = 0, SJENIS = '                      - JUMLAH', TYPEDATA = 'INT', ISSUMMARY = 1, HITUNGKD = 0, SFIELD = '', SMATCH = '', ISISTRI = 0 FROM PEGAWAI&#13;&#10;UNION&#13;&#10;SELECT SORDER = 5, ISRINCIAN = 1, SJENIS = '2. PERINCIAN GAJI', TYPEDATA = 'INT', ISSUMMARY = -1, HITUNGKD = 0, SFIELD = '', SMATCH = '', ISISTRI = 0 FROM PEGAWAI&#13;&#10;UNION&#13;&#10;SELECT SORDER = 6, ISRINCIAN = 1, SJENIS = '   A. GAJI POKOK', TYPEDATA = 'REAL', ISSUMMARY = 0, HITUNGKD = 1, SFIELD = 'GAJI_POKOK', SMATCH = 'SUM', ISISTRI = 0 FROM PEGAWAI&#13;&#10;UNION&#13;&#10;SELECT SORDER = 7, ISRINCIAN = 1, SJENIS = '   B. TUNJANGAN ISTRI / SUAMI', TYPEDATA = 'REAL', ISSUMMARY = 0, HITUNGKD = 1, SFIELD = 'TUNJ_ISTRI', SMATCH = 'SUM', ISISTRI = 0 FROM PEGAWAI&#13;&#10;UNION&#13;&#10;SELECT SORDER = 8, ISRINCIAN = 1, SJENIS = '   C. TUNJANGAN ANAK', TYPEDATA = 'REAL', ISSUMMARY = 0, HITUNGKD = 1, SFIELD = 'TUNJ_ANAK', SMATCH = 'SUM', ISISTRI = 0 FROM PEGAWAI&#13;&#10;UNION&#13;&#10;SELECT SORDER = 9, ISRINCIAN = 1, SJENIS = '   D. TUNJANGAN STRUKTURAL', TYPEDATA = 'REAL', ISSUMMARY = 0, HITUNGKD = 1, SFIELD = 'TUNJ_JAB_STRUKTUR', SMATCH = 'SUM', ISISTRI = 0 FROM PEGAWAI&#13;&#10;UNION&#13;&#10;SELECT SORDER =10, ISRINCIAN = 1, SJENIS = '   