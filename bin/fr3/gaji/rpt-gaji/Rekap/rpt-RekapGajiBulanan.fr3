<TfrxReport Version="3.22" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="40575,6872547685" ReportOptions.Description.Text="" ReportOptions.LastChange="40586,423026794" ScriptLanguage="PascalScript" ScriptText.Text="VAR //JMLSUSUNAN, KOTOR, POT, BERSIH, JMLPERBARIS: DOUBLE;&#13;&#10;       SQLSTR, TMTAHUN, THNGAJI, TMBULAN, BULANGAJI, BULANNM, KEPALA, T1, N1, T2, N2, T3, N3: STRING;&#13;&#10;       LKS, RAPELBLN: STRING;&#13;&#10;       TGL: TDATETIME;&#13;&#10;       SWHERE, JNS: STRING;&#13;&#10;       BANYAK: INTEGER;&#13;&#10;       MULAI, SAMPAI: STRING;&#13;&#10;       SUMSUSUNAN1, SUMSUSUNAN2, SUMSUSUNAN3, SUMSUSUNAN4: INTEGER;&#13;&#10;       SUMKOTOR1, SUMKOTOR2, SUMKOTOR3, SUMKOTOR4: REAL;&#13;&#10;       SUMPOTONGAN1, SUMPOTONGAN2, SUMPOTONGAN3, SUMPOTONGAN4: REAL;&#13;&#10;       LAINLAIN:ARRAY[1..4] OF REAL;&#13;&#10;&#13;&#10;       RAPELNIP: String;&#13;&#10;       bulan:array[0..12] of string;&#13;&#10;&#13;&#10;function HasSubUnit(kode_skpd : string): boolean;&#13;&#10;var kd_skpd:string;&#13;&#10;begin&#13;&#10;  kd_skpd:=trim(kode_skpd);&#13;&#10;  if copy(kd_skpd,length(kd_skpd)-1,2)='00' then&#13;&#10;     kd_skpd:=copy(kd_skpd,0,length(kd_skpd)-2);&#13;&#10;&#13;&#10;  qrySub.Close;&#13;&#10;  ReplaceStr(qrySub.SQL.Text, '&#60;unitkd&#62;', kd_skpd);&#13;&#10;  qrySub.Open;&#13;&#10;  Result:= (&#60;qrySub.&#34;sub_x&#34;&#62; &#62; 1);&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;FUNCTION TGLINDONESIA(T:TDATETIME):STRING;&#13;&#10;VAR H, B, Y:STRING;&#13;&#10;       M: INTEGER;&#13;&#10;BEGIN&#13;&#10;       H:= '0' + INTTOSTR(DAYOF(T));&#13;&#10;       H:= COPY(H,Length(H)-1,2);&#13;&#10;       M:= MONTHOF(T);&#13;&#10;       CASE M OF&#13;&#10;       1: B:='JANUARI';&#13;&#10;       2: B:='FEBRUARI';&#13;&#10;       3: B:='MARET';&#13;&#10;       4: B:='APRIL';&#13;&#10;       5: B:='MEI';&#13;&#10;       6: B:='JUNI';&#13;&#10;       7: B:='JULI';&#13;&#10;       8: B:='AGUSTUS';&#13;&#10;       9: B:='SEPTEMBER';&#13;&#10;       10: B:='OKTOBER';&#13;&#10;       11: B:='NOPEMBER';&#13;&#10;       12: B:='DESEMBER';&#13;&#10;       END;&#13;&#10;       Y:= INTTOSTR(YEAROF(T));&#13;&#10;       RESULT:= H + ' ' + B + ' ' + Y;&#13;&#10;END;&#13;&#10;&#13;&#10;PROCEDURE GETJMLSUSUNAN(VAR TXT: INTEGER; FLD, GOL, SQLMATCH: STRING; LEFTGOL: INTEGER; SIA: INTEGER=0);&#13;&#10;VAR S: STRING;&#13;&#10;BEGIN&#13;&#10;       //showmessage('Di susunan');&#13;&#10;       IF (QRYGAJI.RECORDCOUNT&#60;1) THEN&#13;&#10;               TXT:= 0&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               TMP1.CLOSE;&#13;&#10;               S:= 'SELECT ' + SQLMATCH + '(' + FLD + ') AS JML FROM PEGAWAI_GAJI ' +&#13;&#10;                       'WHERE BULAN=''' + &#60;QRYGAJI.&#34;BULAN&#34;&#62; + ''' AND ' +&#13;&#10;                       'TAHUN=''' + &#60;QRYGAJI.&#34;TAHUN&#34;&#62; + ''' AND JENIS=' + JNS +&#13;&#10;                       ' AND LEFT(GOLONGANKD,' + INTTOSTR(LEFTGOL) + ')=''' + GOL + '''';&#13;&#10;               CASE STRTOINT(JNS) OF&#13;&#10;               0, 1 :&#13;&#10;                       BEGIN&#13;&#10;                               S:= S + ' AND (NOT AKTIF_KD IN(3, 4, 5, 6)) ';&#13;&#10;                       END;&#13;&#10;               6 :&#13;&#10;                       BEGIN&#13;&#10;                               S:= S + ' AND (AKTIF_KD IN(4, 5)) ';&#13;&#10;                       END;&#13;&#10;               END;&#13;&#10;               CASE SIA OF&#13;&#10;               1: S:= S + ' AND JML_SI&#62;0';&#13;&#10;               2: S:= S + ' AND JML_ANAK&#62;0';&#13;&#10;               END;&#13;&#10;               IF (cbIncludeSub.CHECKED) THEN&#13;&#10;                       S:= S + ' AND ((UNITKD=''' + UNITKD + ''') OR (UNITKD LIKE ''' + UNITKD + '%''))'&#13;&#10;               ELSE S:= S + ' AND UNITKD=''' + UNITKD + '''';&#13;&#10;               IF (RAPELNIP&#60;&#62;'') THEN S:= S + ' AND ((nip like ''' + RAPELNIP + '%'') or (nama like ''' + RAPELNIP + '%''))';&#13;&#10;               //SHOWMESSAGE(S);&#13;&#10;               TMP1.SQL.TEXT:= S;&#13;&#10;               try&#13;&#10;               TMP1.OPEN;&#13;&#10;               TXT:= &#60;TMP1.&#34;JML&#34;&#62;;&#13;&#10;               except&#13;&#10;                       //showmessage('Di susunan');&#13;&#10;               end;&#13;&#10;       END;&#13;&#10;END;&#13;&#10;&#13;&#10;&#13;&#10;PROCEDURE GETTOTALRAPEL(VAR TOT: VARIANT; FLD, GOL, SQLMATH: STRING; LEFTGOL: INTEGER; KOL: INTEGER=1);&#13;&#10;VAR&#13;&#10;       S: STRING;&#13;&#10;BEGIN&#13;&#10;       {IF (FLD='LAIN') THEN&#13;&#10;       BEGIN&#13;&#10;               //LAINLAIN[KOL]:= LAINLAIN[KOL] * BANYAK;&#13;&#10;               TOT:= LAINLAIN[KOL];// * BANYAK;&#13;&#10;               EXIT;&#13;&#10;       END; }&#13;&#10;       TMP2.CLOSE;&#13;&#10;       S:= 'SELECT ' + //UNITKD,' +&#13;&#10;               '(SUM(' + FLD + ')) TOTAL ' +&#13;&#10;               ' FROM PEGAWAI_GAJI WHERE ' +&#13;&#10;               ' JENIS=' + JNS + ' AND BULAN=''' + &#60;QRYGAJI.&#34;BULAN&#34;&#62; + '''' +&#13;&#10;               ' AND TAHUN=''' + &#60;QRYGAJI.&#34;TAHUN&#34;&#62; + '''' +&#13;&#10;               ' AND LEFT(GOLONGANKD,' + INTTOSTR(LEFTGOL) + ') = ''' + GOL + '''';&#13;&#10;       IF (cbIncludeSub.CHECKED) THEN&#13;&#10;               S:= S + ' AND ((UNITKD=''' + UNITKD + ''') OR (UNITKD LIKE ''' + UNITKD + '%''))'&#13;&#10;       ELSE S:= S + ' AND UNITKD=''' + UNITKD + '''';&#13;&#10;        CASE STRTOINT(JNS) OF&#13;&#10;               0, 1 :&#13;&#10;                       BEGIN&#13;&#10;                               S:= S + ' AND (NOT AKTIF_KD IN(3, 4, 5, 6)) ';&#13;&#10;                       END;&#13;&#10;               6 :&#13;&#10;                       BEGIN&#13;&#10;                               S:= S + ' AND (AKTIF_KD IN(4, 5)) ';&#13;&#10;                       END;&#13;&#10;               END;&#13;&#10;       IF (ENIP.TEXT&#60;&#62;'') THEN S:= S + ' AND ((A.nip like ''' + RAPELNIP + '%'') or (A.nama like ''' + RAPELNIP + '%''))';&#13;&#10;       //S:= S + ' GROUP BY A.UNITKD';&#13;&#10;       //SHOWMESSAGE(S);&#13;&#10;       TMP2.SQL.TEXT:= S;&#13;&#10;       TMP2.OPEN;&#13;&#10;       TOT:= &#60;TMP2.&#34;TOTAL&#34;&#62;;&#13;&#10;       //LAINLAIN[KOL]:= LAINLAIN[KOL] + &#60;TMP2.&#34;LAIN&#34;&#62;;&#13;&#10;&#13;&#10;END;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;VAR&#13;&#10;       G1, G2, G3, G4, GTOT: REAL;&#13;&#10;       N, GJ, RAPEL: REAL;&#13;&#10;       J, J1, J2, J3, J4, JTOT: INTEGER;&#13;&#10;begin&#13;&#10;       N:= 0; GJ:= 0; RAPEL:= 0;&#13;&#10;       GOL1.TEXT:='';&#13;&#10;       GOL2.TEXT:='';&#13;&#10;       GOL3.TEXT:='';&#13;&#10;       GOL4.TEXT:='';&#13;&#10;       TOTALGOL.TEXT:='';&#13;&#10;       IF (&#60;QRYREKAP.&#34;ISSUMMARY&#34;&#62;=0) THEN&#13;&#10;       BEGIN&#13;&#10;               IF (&#60;QRYREKAP.&#34;ISRINCIAN&#34;&#62;=0)  THEN&#13;&#10;               BEGIN&#13;&#10;                       GETJMLSUSUNAN(J, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'I/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 2, &#60;QRYREKAP.&#34;ISISTRI&#34;&#62;);&#13;&#10;                       GOL1.TEXT:= INTTOSTR(J);&#13;&#10;                       J1:= J;&#13;&#10;                       SUMSUSUNAN1:= SUMSUSUNAN1 + J1;&#13;&#10;                       GETJMLSUSUNAN(J, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'II/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 3, &#60;QRYREKAP.&#34;ISISTRI&#34;&#62;);&#13;&#10;                       GOL2.TEXT:= INTTOSTR(J);&#13;&#10;                       J2:= J;&#13;&#10;                       SUMSUSUNAN2:= SUMSUSUNAN2 + J2;&#13;&#10;                       GETJMLSUSUNAN(J, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'III/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 4, &#60;QRYREKAP.&#34;ISISTRI&#34;&#62;);&#13;&#10;                       GOL3.TEXT:= INTTOSTR(J);&#13;&#10;                       J3:= J;&#13;&#10;                       SUMSUSUNAN3:= SUMSUSUNAN3 + J3;&#13;&#10;                       GETJMLSUSUNAN(J, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'IV/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 3, &#60;QRYREKAP.&#34;ISISTRI&#34;&#62;);&#13;&#10;                       GOL4.TEXT:= INTTOSTR(J);&#13;&#10;                       J4:= J;&#13;&#10;                       SUMSUSUNAN4:= SUMSUSUNAN4 + J4;&#13;&#10;                       TOTALGOL.TEXT:= INTTOSTR(J1+J2+J3+J4);&#13;&#10;               END&#13;&#10;               ELSE&#13;&#10;               BEGIN&#13;&#10;                       GETTOTALRAPEL(G1, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'I/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 2, 1);&#13;&#10;                       GOL1.TEXT:= FORMATFLOAT('#,##0', G1);&#13;&#10;                       IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=1) THEN&#13;&#10;                               SUMKOTOR1:= SUMKOTOR1 + G1&#13;&#10;                       ELSE IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=-1) THEN&#13;&#10;                               SUMPOTONGAN1:= SUMPOTONGAN1 + G1;&#13;&#10;&#13;&#10;                       GETTOTALRAPEL(G2, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'II/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 3, 2);&#13;&#10;&#13;&#10;                       GOL2.TEXT:=  FORMATFLOAT('#,##0', G2);&#13;&#10;                       IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=1) THEN&#13;&#10;                               SUMKOTOR2:= SUMKOTOR2 + G2&#13;&#10;                       ELSE IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=-1) THEN&#13;&#10;                               SUMPOTONGAN2:= SUMPOTONGAN2 + G2;&#13;&#10;&#13;&#10;                       GETTOTALRAPEL(G3, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'III/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 4, 3);&#13;&#10;&#13;&#10;                       GOL3.TEXT:= FORMATFLOAT('#,##0', G3);&#13;&#10;                       IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=1) THEN&#13;&#10;                               SUMKOTOR3:= SUMKOTOR3 + G3&#13;&#10;                       ELSE IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=-1) THEN&#13;&#10;                               SUMPOTONGAN3:= SUMPOTONGAN3 + G3;&#13;&#10;&#13;&#10;                       GETTOTALRAPEL(G4, &#60;QRYREKAP.&#34;SFIELD&#34;&#62;, 'IV/', &#60;QRYREKAP.&#34;SMATCH&#34;&#62;, 3, 4);&#13;&#10;&#13;&#10;                       GOL4.TEXT:= FORMATFLOAT('#,##0', G4);&#13;&#10;                       IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=1) THEN&#13;&#10;                               SUMKOTOR4:= SUMKOTOR4 + G4&#13;&#10;                       ELSE IF (&#60;QRYREKAP.&#34;HITUNGKD&#34;&#62;=-1) THEN&#13;&#10;                               SUMPOTONGAN4:= SUMPOTONGAN4 + G4;&#13;&#10;                       TOTALGOL.TEXT:= FORMATFLOAT('#,##0', G1 + G2 + G3 + G4);&#13;&#10;               END;&#13;&#10;       END&#13;&#10;       ELSE IF (&#60;QRYREKAP.&#34;ISSUMMARY&#34;&#62;=1) THEN&#13;&#10;       BEGIN&#13;&#10;               IF (&#60;QRYREKAP.&#34;ISRINCIAN&#34;&#62;=0)  THEN&#13;&#10;               BEGIN&#13;&#10;                       GOL1.TEXT:= INTTOSTR(SUMSUSUNAN1);&#13;&#10;                       GOL2.TEXT:= INTTOSTR(SUMSUSUNAN2);&#13;&#10;                       GOL3.TEXT:= INTTOSTR(SUMSUSUNAN3);&#13;&#10;                       GOL4.TEXT:= INTTOSTR(SUMSUSUNAN4);&#13;&#10;                       TOTALGOL.TEXT:= INTTOSTR(SUMSUSUNAN1+SUMSUSUNAN2+SUMSUSUNAN3+SUMSUSUNAN4);&#13;&#10;               END&#13;&#10;               ELSE&#13;&#10;               BEGIN&#13;&#10;                       CASE &#60;QRYREKAP.&#34;HITUNGKD&#34;&#62; OF&#13;&#10;                       1 :&#13;&#10;                               BEGIN&#13;&#10;                                       GOL1.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR1);&#13;&#10;                                       GOL2.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR2);&#13;&#10;                                       GOL3.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR3);&#13;&#10;                                       GOL4.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR4);&#13;&#10;                                       TOTALGOL.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR1+SUMKOTOR2+SUMKOTOR3+SUMKOTOR4);&#13;&#10;                               END;&#13;&#10;                       2 :&#13;&#10;                               BEGIN&#13;&#10;                                       GOL1.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR1-SUMPOTONGAN1);&#13;&#10;                                       GOL2.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR2-SUMPOTONGAN2);&#13;&#10;                                       GOL3.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR3-SUMPOTONGAN3);&#13;&#10;                                       GOL4.TEXT:= FORMATFLOAT('#,##0', SUMKOTOR4-SUMPOTONGAN4);&#13;&#10;                                       TOTALGOL.TEXT:= FORMATFLOAT('#,##0', (SUMKOTOR1-SUMPOTONGAN1)+(SUMKOTOR2-SUMPOTONGAN2)+(SUMKOTOR3-SUMPOTONGAN3)+(SUMKOTOR4-SUMPOTONGAN4));&#13;&#10;                               END;&#13;&#10;                       -1 :&#13;&#10;                               BEGIN&#13;&#10;                                       GOL1.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN1);&#13;&#10;                                       GOL2.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN2);&#13;&#10;                                       GOL3.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN3);&#13;&#10;                                       GOL4.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN4);&#13;&#10;                                       TOTALGOL.TEXT:= FORMATFLOAT('#,##0', SUMPOTONGAN1+SUMPOTONGAN2+SUMPOTONGAN3+SUMPOTONGAN4);&#13;&#10;                               END;&#13;&#10;                       END;&#13;&#10;               END;&#13;&#10;       END;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;VAR    STRWHERE: STRING;&#13;&#10;       D1, D2: STRING;&#13;&#10;begin&#13;&#10;&#13;&#10;       TGL:= TANGGAL.DATE;&#13;&#10;&#13;&#10;       IF (cbBulan.TEXT)='' THEN&#13;&#10;               BULANGAJI:= ''&#13;&#10;       ELSE&#13;&#10;       BEGIN&#13;&#10;               BULANGAJI:= '0' + INTTOSTR(cbBulan.ITEMINDEX+1);&#13;&#10;               BULANGAJI:= COPY(BULANGAJI, Length(BULANGAJI)-1, 2);&#13;&#10;       END;&#13;&#10;       IF (RBGAJI13.CHECKED) THEN JNS:= '1'&#13;&#10;       ELSE IF (RBGAJITERUS.CHECKED) THEN JNS:= '6'&#13;&#10;       ELSE JNS:= '0';&#13;&#10;       THNGAJI:= cbTahun.KEYVALUE;&#13;&#10;       STRWHERE := 'BULAN=''' + BULANGAJI + ''' AND TAHUN=''' + THNGAJI + '''' +&#13;&#10;               ' AND JENIS=' + JNS;&#13;&#10;       QRYGAJI.CLOSE;&#13;&#10;       REPLACESTR(QRYGAJI.SQL.TEXT, '1=1', STRWHERE);&#13;&#10;       QRYGAJI.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;var i:integer;&#13;&#10;begin&#13;&#10;       TANGGAL.DATE:= DATE;&#13;&#10;       //cbIncludeSub.Visible:= HasSubUnit(UnitKd);&#13;&#10;  cbTahun.KeyValue:=YearOf(Now);&#13;&#10;&#13;&#10;  cbBulan.Items.Clear;&#13;&#10;  for i := 1 to 12 do&#13;&#10;    cbBulan.Items.Add( Bulan[i] );&#13;&#10;  cbBulan.ItemIndex := MonthOf(Date) - 1;&#13;&#10;&#13;&#10;  //dtTanggal.Date := Date;&#13;&#10;  qrySKPD.Close;&#13;&#10;  qrySKPD.Params[0].Value:=UnitKd;&#13;&#10;  qrySKPD.Open;&#13;&#10;&#13;&#10;  cbIncludeSub.Visible:= HasSubUnit(UnitKd);&#13;&#10;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;       SUMSUSUNAN1:= 0;&#13;&#10;       SUMSUSUNAN2:= 0;&#13;&#10;       SUMSUSUNAN3:= 0;&#13;&#10;       SUMSUSUNAN4:= 0;&#13;&#10;       SUMKOTOR1:= 0;&#13;&#10;       SUMKOTOR2:= 0;&#13;&#10;       SUMKOTOR3:= 0;&#13;&#10;       SUMKOTOR4:= 0;&#13;&#10;       SUMPOTONGAN1:= 0;&#13;&#10;       SUMPOTONGAN2:= 0;&#13;&#10;       SUMPOTONGAN3:= 0;&#13;&#10;       SUMPOTONGAN4:= 0;&#13;&#10;       QRYTAHUN.OPEN;&#13;&#10;&#13;&#10;       QRYREKAP.OPEN;&#13;&#10;       QRYSKPD.CLOSE;&#13;&#10;       QRYSKPD.PARAMS[0].VALUE:= UNITKD;&#13;&#10;       QRYSKPD.OPEN;&#13;&#10;&#13;&#10;       QRYProperti.Close;&#13;&#10;       QRYProperti.SQL.Text:= 'SELECT * FROM UNITTAHUN WHERE TAHUN=''' + TahunAnggaran + '''' +&#13;&#10;                       ' AND UnitKd=''' + UnitKd + '''';&#13;&#10;       QRYProperti.Open;&#13;&#10;&#13;&#10;  Bulan[00] := '';&#13;&#10;  Bulan[01] := 'Januari';&#13;&#10;  Bulan[02] := 'Februari';&#13;&#10;  Bulan[03] := 'Maret';&#13;&#10;  Bulan[04] := 'April';&#13;&#10;  Bulan[05] := 'Mei';&#13;&#10;  Bulan[06] := 'Juni';&#13;&#10;  Bulan[07] := 'Juli';&#13;&#10;  Bulan[08] := 'Agustus';&#13;&#10;  Bulan[09] := 'September';&#13;&#10;  Bulan[10] := 'Oktober';&#13;&#10;  Bulan[11] := 'November';&#13;&#10;  Bulan[12] := 'Desember';&#13;&#10;&#13;&#10;  //zeroVal := 0;&#13;&#10;&#13;&#10;end." PropData="">
