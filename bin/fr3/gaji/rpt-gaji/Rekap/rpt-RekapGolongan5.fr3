    <TfrxADOQuery Name="qryGaji" UserName="qryGaji" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select Top 1 'I' as grp, 0 as sub, 'JUMLAH PENERIMA (ORANG)' as uraian,&#13;&#10;       (SELECT COUNT(*)+SUM(jml_si)+SUM(jml_anak)  FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I' and gaji_pokok&#62;0 and gurukd='85') AS Gol1,&#13;&#10;       (SELECT COUNT(*)+SUM(jml_si)+SUM(jml_anak) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II' and gaji_pokok&#62;0 and gurukd='85') AS Gol2,&#13;&#10;       (SELECT COUNT(*)+SUM(jml_si)+SUM(jml_anak) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III' and gaji_pokok&#62;0 and gurukd='85') AS Gol3,&#13;&#10;       (SELECT COUNT(*)+SUM(jml_si)+SUM(jml_anak) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='IV' and gaji_pokok&#62;0 and gurukd='85') AS Gol4,&#13;&#10;		' 1. PFK BULOG' AS POT,&#13;&#10;		0 AS POTVALUE&#13;&#10;&#13;&#10;FROM Pegawai_gaji PG&#13;&#10;WHERE 1=1 --and jenis=0 and bulan=1 and tahun=2011&#13;&#10;UNION&#13;&#10;select Top 1 'I' as grp, 1 as sub, '1. PEGAWAI' as uraian,&#13;&#10;       (SELECT COUNT(*)  FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I' and gaji_pokok&#62;0 and gurukd='85') AS Gol1,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II' and gaji_pokok&#62;0 and gurukd='85') AS Gol2,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III' and gaji_pokok&#62;0 and gurukd='85') AS Gol3,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='IV' and gaji_pokok&#62;0 and gurukd='85') AS Gol4,&#13;&#10;		' 2. PFK 10% (IWP)' AS POT,&#13;&#10;		(SELECT SUM(POT_IWP) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun) AS POTVALUE&#13;&#10;FROM Pegawai_gaji PG&#13;&#10;WHERE 1=1 --and jenis=0 and bulan=1 and tahun=2011&#13;&#10;&#13;&#10;UNION --ISTRI&#13;&#10;select Top 1 'I' as grp, 2 as sub, '2. ISTRI/SUAMI' as uraian,&#13;&#10;       (SELECT SUM(jml_si) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I' and gaji_pokok&#62;0 and gurukd='85') AS Gol1,&#13;&#10;       (SELECT SUM(jml_si) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II' and gaji_pokok&#62;0 and gurukd='85') AS Gol2,&#13;&#10;       (SELECT SUM(jml_si) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III' and gaji_pokok&#62;0 and gurukd='85') AS Gol3,&#13;&#10;       (SELECT SUM(jml_si) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='IV' and gaji_pokok&#62;0 and gurukd='85') AS Gol4,&#13;&#10;		' 3. SEWA RUMAH' AS POT,&#13;&#10;		0 AS POTVALUE&#13;&#10;FROM Pegawai_gaji PG&#13;&#10;WHERE 1=1 --and jenis=0 and bulan=1 and tahun=2011&#13;&#10;UNION  --ANAK&#13;&#10;select Top  1 'I' as grp, 3 as sub, '3. ANAK' as uraian,&#13;&#10;       (SELECT SUM(jml_anak) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I' and gaji_pokok&#62;0 and gurukd='85') AS Gol1,&#13;&#10;       (SELECT SUM(jml_anak) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II' and gaji_pokok&#62;0 and gurukd='85') AS Gol2,&#13;&#10;       (SELECT SUM(jml_anak) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III' and gaji_pokok&#62;0 and gurukd='85') AS Gol3,&#13;&#10;       (SELECT SUM(jml_anak) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='IV' and gaji_pokok&#62;0 and gurukd='85') AS Gol4,&#13;&#10;		' 4. HUTANG KELEBIHAN' AS POT,&#13;&#10;		0 AS POTVALUE&#13;&#10;FROM Pegawai_gaji PG&#13;&#10;WHERE 1=1 --and jenis=0 and bulan=1 and tahun=2011&#13;&#10;UNION&#13;&#10;select Top  1 'II' as grp, 0 as sub, 'JUMLAH PENERIMA TUNJANGAN' as uraian,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I'&#13;&#10;               and ISNULL(JBT_STRUKTURKD,0)&#60;99 AND ISNULL(JBT_STRUKTURKD,0)&#62;0 and gaji_pokok&#62;0 and gurukd='85')&#13;&#10;      +(SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I'&#13;&#10;               and ISNULL(JBT_FUNGSIKD,'')&#60;&#62;''  and gaji_pokok&#62;0 and gurukd='85') AS Gol1,&#13;&#10;&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II'&#13;&#10;               and ISNULL(JBT_STRUKTURKD,0)&#60;99 AND ISNULL(JBT_STRUKTURKD,0)&#62;0 and gaji_pokok&#62;0 and gurukd='85')&#13;&#10;      +(SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II'&#13;&#10;               and ISNULL(JBT_FUNGSIKD,'')&#60;&#62;''  and gaji_pokok&#62;0 and gurukd='85') AS Gol2,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III'&#13;&#10;                and ISNULL(JBT_STRUKTURKD,0)&#60;99 AND ISNULL(JBT_STRUKTURKD,0)&#62;0 and gaji_pokok&#62;0 and gurukd='85')&#13;&#10;      +(SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III'&#13;&#10;               and ISNULL(JBT_FUNGSIKD,'')&#60;&#62;''  and gaji_pokok&#62; 0 and gurukd='85') AS Gol3,&#13;&#10;      (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='IV'&#13;&#10;               and ISNULL(JBT_STRUKTURKD,0)&#60;61 AND ISNULL(JBT_STRUKTURKD,0)&#62;0  and gaji_pokok&#62;0 and gurukd='85')&#13;&#10;      +(SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='IV'&#13;&#10;               and ISNULL(JBT_FUNGSIKD,'')&#60;&#62;''  and gaji_pokok&#62;0 and gurukd='85') AS Gol4,&#13;&#10;		'    PENERIMAAN' AS POT,&#13;&#10;		0 AS POTVALUE&#13;&#10;FROM Pegawai_gaji PG&#13;&#10;WHERE 1=1 --and jenis=0 and bulan=1 and tahun=2011&#13;&#10;UNION --STRUKTUR&#13;&#10;select Top  1 'II' as grp, 1 as sub, '1. STRUKTURAL' as uraian,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I'&#13;&#10;               and ISNULL(JBT_STRUKTURKD,0)&#60;99 AND ISNULL(JBT_STRUKTURKD,0)&#62;0  and gaji_pokok&#62;0 and gurukd='85')AS Gol1,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II'&#13;&#10;               and ISNULL(JBT_STRUKTURKD,0)&#60;99 AND ISNULL(JBT_STRUKTURKD,0)&#62;0  and gaji_pokok&#62;0 and gurukd='85')AS Gol2,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III'&#13;&#10;                and ISNULL(JBT_STRUKTURKD,0)&#60;99 AND ISNULL(JBT_STRUKTURKD,0)&#62;0  and gaji_pokok&#62;0 and gurukd='85')AS Gol3,&#13;&#10;      (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='IV'&#13;&#10;               and ISNULL(JBT_STRUKTURKD,0)&#60;61 AND ISNULL(JBT_STRUKTURKD,0)&#62;0  and gaji_pokok&#62;0 and gurukd='85')AS Gol4,&#13;&#10;		' 5. PFK LAIN-LAIN' AS POT,&#13;&#10;		0 AS POTVALUE&#13;&#10;FROM Pegawai_gaji PG&#13;&#10;WHERE 1=1 --and jenis=0 and bulan=1 and tahun=2011&#13;&#10;UNION --FUNGSIONAL&#13;&#10;select Top 1 'II' as grp, 2 as sub, '2. FUNGSIONAL' as uraian,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I'&#13;&#10;               and ISNULL(JBT_FUNGSIKD,'')&#60;&#62;''  and gaji_pokok&#62;0 and gurukd='85')AS Gol1,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II'&#13;&#10;               and ISNULL(JBT_FUNGSIKD,'')&#60;&#62;''  and gaji_pokok&#62;0 and gurukd='85')AS Gol2,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III'&#13;&#10;                and ISNULL(JBT_FUNGSIKD,'')&#60;&#62;''  and gaji_pokok&#62;0 and gurukd='85')AS Gol3,&#13;&#10;      (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='IV'&#13;&#10;               and ISNULL(JBT_FUNGSIKD,'')&#60;&#62;''  and gaji_pokok&#62;0 and gurukd='85')AS Gol4,&#13;&#10;		' 6. PENERIMAAN ANGGARAN' AS POT,&#13;&#10;		0 AS POTVALUE&#13;&#10;FROM Pegawai_gaji PG&#13;&#10;WHERE 1=1 --and jenis=0 and bulan=1 and tahun=2011&#13;&#10;UNION --UMUM&#13;&#10;select Top 1 'II' as grp, 3 as sub, '3. UMUM' as uraian,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I'&#13;&#10;               and ISNULL(TUNJ_UMUM,0)&#62;0  and gaji_pokok&#62;0 and gurukd='85')AS Gol1,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II'&#13;&#10;               and ISNULL(TUNJ_UMUM,0)&#62;0  and gaji_pokok&#62;0 and gurukd='85')AS Gol2,&#13;&#10;       (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III'&#13;&#10;                and ISNULL(TUNJ_UMUM,0)&#62;0  and gaji_pokok&#62;0 and gurukd='85')AS Gol3,&#13;&#10;      (SELECT COUNT(*) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='IV'&#13;&#10;               and ISNULL(TUNJ_UMUM,0)&#62;0   and gaji_pokok&#62;0 and gurukd='85')AS Gol4,&#13;&#10;		' 7. PPH' AS POT,&#13;&#10;		(SELECT SUM(PPH) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun) AS POTVALUE&#13;&#10;FROM Pegawai_gaji PG&#13;&#10;WHERE 1=1 --and jenis=0 and bulan=1 and tahun=2011&#13;&#10;UNION --GAJI POKOK&#13;&#10;select Top 1 'III' as grp, 0 as sub, 'JUMLAH GAJI DAN TUNJANGAN' as uraian,&#13;&#10;       (SELECT SUM(GAJI_POKOK+TUNJ_ISTRI+TUNJ_ANAK+TUNJ_JAB_STRUKTUR+TUNJ_JAB_FUNGSI+&#13;&#10;                   TUNJ_UMUM+PPH+PEMBULATAN+TUNJ_ASKES) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='I'  and gurukd='85')AS Gol1,&#13;&#10;&#13;&#10;       (SELECT SUM(GAJI_POKOK+TUNJ_ISTRI+TUNJ_ANAK+TUNJ_JAB_STRUKTUR+TUNJ_JAB_FUNGSI+&#13;&#10;                   TUNJ_UMUM+PPH+PEMBULATAN+TUNJ_ASKES) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='II' and gurukd='85')AS Gol2,&#13;&#10;&#13;&#10;       (SELECT SUM(GAJI_POKOK+TUNJ_ISTRI+TUNJ_ANAK+TUNJ_JAB_STRUKTUR+TUNJ_JAB_FUNGSI+&#13;&#10;                   TUNJ_UMUM+PPH+PEMBULATAN+TUNJ_ASKES) FROM Pegawai_gaji&#13;&#10;         WHERE jenis=pg.jenis and bulan=pg.bulan and tahun=pg.tahun&#13;&#10;               and LEFT(GolonganKd,Len(GolonganKd)-2)='III'  and gurukd='85')AS Gol3,&#13;&#10;&#13;&#10;       (SELECT SUM(GAJI_POKOK+TUNJ_ISTRI+TUNJ_ANAK+TUNJ_JAB_STRUKTUR+TUNJ_JAB_FUNGSI+&#13;&#10;                   TUNJ_UMUM+PPH+PEMBULATAN+TUNJ_ASKE