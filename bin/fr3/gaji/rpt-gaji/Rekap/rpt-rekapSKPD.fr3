    <TfrxADOQuery Name="qryGaji" UserName="qryGaji" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT   u.UnitKd, u.UnitNm, u.UnitKd AS u2, COUNT(*) AS jml, SUM(pg.GAJI_POKOK) AS gapok, SUM(pg.JML_SI) AS jmlsi, SUM(pg.JML_ANAK) AS jmlanak,&#13;&#10;                      SUM(pg.TUNJ_ISTRI) AS tistri, SUM(pg.TUNJ_ANAK) AS tanak, SUM(pg.TUNJ_BERAS)&#13;&#10;                      AS tberas, SUM(pg.TUNJ_KERJA) AS tkerja, SUM(pg.POT_IWP) AS tiwp, SUM(pg.POT_TAPERUM) AS taperum, SUM(pg.POT_SEWA_RUMAH) AS sewa,&#13;&#10;                      SUM(pg.POT_PANGAN) AS pangan, SUM(pg.POT_KORPRI) AS korpti, SUM(pg.POT_GAJI_LEBIH) AS gajilebih, SUM(pg.POT_HUTANG) AS hutan,&#13;&#10;                      SUM(pg.PEMBULATAN) AS bulat, SUM(pg.PPH) AS pajak, SUM(pg.TUNJ_UMUM) AS umum, SUM(pg.TUNJ_UMUM_TAMB) AS umumtamb,&#13;&#10;                      SUM(pg.TUNJ_OTSUS) AS otsus, SUM(pg.TUNJ_DT) AS dt, SUM(pg.TUNJ_ASKES) AS askes, SUM(pg.TUNJ_PENGHASILAN) AS penghasilan,&#13;&#10;                      SUM(pg.BIAYA_JABATAN) AS jabatn, SUM(pg.BIAYA_PENSIUN) AS pensiun, SUM(pg.TUNJ_JAB_FUNGSI)  as fungsi, SUM(pg.TUNJ_JAB_STRUKTUR)  as struk&#13;&#10;FROM         tblUnit AS u INNER JOIN&#13;&#10;                      PEGAWAI_GAJI AS pg ON u.UnitKd = pg.UNITKD&#13;&#10;&#13;&#10;WHERE     1=1 --pg.TAHUN = '2011' AND pg.BULAN = '04' --AND LEFT(pg.UNITKD,4)='4027'&#13;&#10;          AND pg.GAJI_POKOK &#62; 0  and pg.JENIS=0&#13;&#10;          AND (LEFT(u.UnitKd, LEN(u.UnitKd) - 2) NOT IN&#13;&#10;                          (SELECT     LEFT(UnitKd, LEN(UnitKd) - 2) AS Expr1&#13;&#10;                            FROM          tblUnit AS tblUnit_1&#13;&#10;                            WHERE      (RIGHT(UnitKd, 2) = '00') AND (LEN(UnitKd) = 9)&#13;&#10;                            GROUP BY UnitKd, UnitNm))&#13;&#10;GROUP BY u.UnitKd, u.UnitNm&#13;&#10;UNION&#13;&#10;SELECT  u.UnitKd, u.UnitNm, u.UnitKd AS u2, COUNT(*) AS jml, SUM(pg.GAJI_POKOK) AS gapok, SUM(pg.JML_SI) AS jmlsi, SUM(pg.JML_ANAK) AS jmlanak,&#13;&#10;                       SUM(pg.TUNJ_ISTRI) AS tistri, SUM(pg.TUNJ_ANAK) AS tanak, SUM(pg.TUNJ_BERAS)&#13;&#10;                      AS tberas, SUM(pg.TUNJ_KERJA) AS tkerja, SUM(pg.POT_IWP) AS tiwp, SUM(pg.POT_TAPERUM) AS taperum, SUM(pg.POT_SEWA_RUMAH) AS sewa,&#13;&#10;                      SUM(pg.POT_PANGAN) AS pangan, SUM(pg.POT_KORPRI) AS korpti, SUM(pg.POT_GAJI_LEBIH) AS gajilebih, SUM(pg.POT_HUTANG) AS hutan,&#13;&#10;                      SUM(pg.PEMBULATAN) AS bulat, SUM(pg.PPH) AS pajak, SUM(pg.TUNJ_UMUM) AS umum, SUM(pg.TUNJ_UMUM_TAMB) AS umumtamb,&#13;&#10;                      SUM(pg.TUNJ_OTSUS) AS otsus, SUM(pg.TUNJ_DT) AS dt, SUM(pg.TUNJ_ASKES) AS askes, SUM(pg.TUNJ_PENGHASILAN) AS penghasilan,&#13;&#10;                      SUM(pg.BIAYA_JABATAN) AS jabatn, SUM(pg.BIAYA_PENSIUN) AS pensiun, SUM(pg.TUNJ_JAB_FUNGSI)  as fungsi, SUM(pg.TUNJ_JAB_STRUKTUR)  as struk&#13;&#10;FROM         tblUnit AS u INNER JOIN&#13;&#10;                      PEGAWAI_GAJI AS pg ON pg.UNITKD LIKE LEFT(u.UnitKd, LEN(u.UnitKd) - 2) + '%'&#13;&#10;WHERE     1=1 --pg.TAHUN = '2011' AND pg.BULAN = '04' --AND LEFT(pg.UNITKD,4)='4027'&#13;&#10;          AND pg.JENIS=0 AND pg.GAJI_POKOK &#62; 0&#13;&#10;		  AND (RIGHT(u.UnitKd, 2) = '00') AND (LEN(u.UnitKd) = 9)  and len(pg.unitkd)=9&#13;&#10;GROUP BY u.UnitKd, u.UnitNm&#13;&#10;Order by u.UnitKd, u.UnitNm" CommandTimeout="30" PropData="0A506172616D65746572730100"/>
