<TfrxReport Version="4.0.11" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40133.6207497801" ReportOptions.Description.Text="" ReportOptions.LastChange="40134.4352815741" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       qrySKPD.Close;&#13;&#10;       qrySKPD.Open;&#13;&#10;       LookSKPD.Enabled:= rPilih.Checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnOKOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       qryRekapGol.Close;&#13;&#10;       qryRekapGol.Open;&#13;&#10;       qryRekapGol.Filtered:= False;&#13;&#10;       if (rPilih.Checked) then&#13;&#10;       begin&#13;&#10;               if not(LookSKPD.KeyValue=null) then&#13;&#10;               begin&#13;&#10;                       qryRekapGol.Filter:= 'UnitKd=''' + LookSKPD.KeyValue + '''';&#13;&#10;                       qryRekapGol.Filtered:= True;&#13;&#10;               end;&#13;&#10;       end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rSemuaOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       LookSKPD.Enabled:= False;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rPilihOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       LookSKPD.Enabled:= True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       ReportSummary1.Visible:= not qryRekapGol.Filtered;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="044C65667403C80203546F70023808446174617365747301010C3000000020446174615365743D2271727952656B6170476F6C2220446174615365744E616D653D2271727952656B6170476F6C2200010C2800000020446174615365743D22717279534B50442220446174615365744E616D653D22717279534B5044220000095661726961626C65730100055374796C650100">
