    <TfrxADOQuery Name="qryRekapGol" UserName="qryRekapGol" CloseDataSource="True" FieldAliases.Text="" SQL.Text="&#13;&#10;SELECT A.UNITKD, A.UNITNM, A.GOL, JA.JML_A, JB.JML_B, JC.JML_C, JD.JML_D, JE.JML_E,&#13;&#10;	JML= (CASE WHEN JA.JML_A IS NULL THEN 0 ELSE JA.JML_A END) +&#13;&#10;	(CASE WHEN JB.JML_B IS NULL THEN 0 ELSE JB.JML_B END) +&#13;&#10;	(CASE WHEN JC.JML_C IS NULL THEN 0 ELSE JC.JML_C END) +&#13;&#10;	(CASE WHEN JD.JML_D IS NULL THEN 0 ELSE JD.JML_D END) +&#13;&#10;	(CASE WHEN JE.JML_E IS NULL THEN 0 ELSE JE.JML_E END)&#13;&#10;FROM&#13;&#10;	(SELECT DISTINCT REPLACE(G.GOLONGANKD, RIGHT(G.GOLONGANKD,2), '')GOL, P.UNITKD, U.UNITNM&#13;&#10;		FROM TBLGOLONGAN G INNER JOIN PEGAWAI P ON G.GOLONGANKD=P.GOLONGANKD&#13;&#10;		INNER JOIN TBLUNIT U ON P.UNITKD=U.UNITKD&#13;&#10;		WHERE REPLACE(G.GOLONGANKD, RIGHT(G.GOLONGANKD,2), '')!='') A&#13;&#10;	LEFT OUTER JOIN&#13;&#10;	(SELECT U.UNITKD, REPLACE(G.GOLONGANKD, RIGHT(G.GOLONGANKD,2), '')GOL,&#13;&#10;		COUNT(P.GOLONGANKD)JML_A&#13;&#10;		FROM PEGAWAI P INNER JOIN TBLGOLONGAN G ON P.GOLONGANKD=G.GOLONGANKD&#13;&#10;		INNER JOIN TBLUNIT U ON P.UNITKD=U.UNITKD&#13;&#10;		WHERE RIGHT(G.GOLONGANKD,1)='A'&#13;&#10;		GROUP BY U.UNITKD, G.GOLONGANKD) JA&#13;&#10;ON A.GOL=JA.GOL AND A.UNITKD=JA.UNITKD&#13;&#10;	LEFT OUTER JOIN&#13;&#10;	(SELECT U.UNITKD, REPLACE(G.GOLONGANKD, RIGHT(G.GOLONGANKD,2), '')GOL,&#13;&#10;		COUNT(P.GOLONGANKD)JML_B&#13;&#10;		FROM PEGAWAI P INNER JOIN TBLGOLONGAN G ON P.GOLONGANKD=G.GOLONGANKD&#13;&#10;		INNER JOIN TBLUNIT U ON P.UNITKD=U.UNITKD&#13;&#10;		WHERE RIGHT(G.GOLONGANKD,1)='b'&#13;&#10;		GROUP BY U.UNITKD, G.GOLONGANKD) JB&#13;&#10;ON A.GOL=JB.GOL AND A.UNITKD=JB.UNITKD&#13;&#10;LEFT OUTER JOIN&#13;&#10;	(SELECT U.UNITKD, REPLACE(G.GOLONGANKD, RIGHT(G.GOLONGANKD,2), '')GOL,&#13;&#10;		COUNT(P.GOLONGANKD)JML_C&#13;&#10;		FROM PEGAWAI P INNER JOIN TBLGOLONGAN G ON P.GOLONGANKD=G.GOLONGANKD&#13;&#10;		INNER JOIN TBLUNIT U ON P.UNITKD=U.UNITKD&#13;&#10;		WHERE RIGHT(G.GOLONGANKD,1)='C'&#13;&#10;		GROUP BY U.UNITKD, G.GOLONGANKD) JC&#13;&#10;ON A.GOL=JC.GOL AND A.UNITKD=JC.UNITKD&#13;&#10;LEFT OUTER JOIN&#13;&#10;	(SELECT U.UNITKD, REPLACE(G.GOLONGANKD, RIGHT(G.GOLONGANKD,2), '')GOL,&#13;&#10;		COUNT(P.GOLONGANKD)JML_D&#13;&#10;		FROM PEGAWAI P INNER JOIN TBLGOLONGAN G ON P.GOLONGANKD=G.GOLONGANKD&#13;&#10;		INNER JOIN TBLUNIT U ON P.UNITKD=U.UNITKD&#13;&#10;		WHERE RIGHT(G.GOLONGANKD,1)='D'&#13;&#10;		GROUP BY U.UNITKD, G.GOLONGANKD) JD&#13;&#10;ON A.GOL=JD.GOL AND A.UNITKD=JD.UNITKD&#13;&#10;LEFT OUTER JOIN&#13;&#10;	(SELECT U.UNITKD, REPLACE(G.GOLONGANKD, RIGHT(G.GOLONGANKD,2), '')GOL,&#13;&#10;		COUNT(P.GOLONGANKD)JML_E&#13;&#10;		FROM PEGAWAI P INNER JOIN TBLGOLONGAN G ON P.GOLONGANKD=G.GOLONGANKD&#13;&#10;		INNER JOIN TBLUNIT U ON P.UNITKD=U.UNITKD&#13;&#10;		WHERE RIGHT(G.GOLONGANKD,1)='E'&#13;&#10;		GROUP BY U.UNITKD, G.GOLONGANKD) JE&#13;&#10;ON A.GOL=JE.GOL AND A.UNITKD=JE.UNITKD&#13;&#10;&#13;&#10;ORDER BY A.UNITKD, A.GOL" CommandTimeout="30" PropData="05704C6566740360010470546F7002540A506172616D65746572730100"/>
