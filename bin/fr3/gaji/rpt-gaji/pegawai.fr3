<TfrxReport Version="3.22" DotMatrixReport="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="40575.6013858102" ReportOptions.Description.Text="" ReportOptions.LastChange="40593.4160539583" ScriptLanguage="PascalScript" ScriptText.Text="VAR&#13;&#10;       SQLGOL:STRING;&#13;&#10;&#13;&#10;&#13;&#10;procedure ReplaceStr(var Source: string; ToReplace, ReplaceWith: string);&#13;&#10;var&#13;&#10;  x : integer;&#13;&#10;begin&#13;&#10;  x := Pos(ToReplace, Source);&#13;&#10;  while x &#62; 0 do&#13;&#10;  begin&#13;&#10;    DeleteStr(Source, x, length(ToReplace));&#13;&#10;    Insert(ReplaceWith, Source, x);&#13;&#10;    x := Pos(ToReplace, Source);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;function HasSubUnit(kode_skpd : string): boolean;&#13;&#10;var kd_skpd:string;&#13;&#10;begin&#13;&#10;  kd_skpd:=trim(kode_skpd);&#13;&#10;  if copy(kd_skpd,length(kd_skpd)-1,2)='00' then&#13;&#10;     kd_skpd:=copy(kd_skpd,0,length(kd_skpd)-2);&#13;&#10;&#13;&#10;  qrySub.Close;&#13;&#10;  ReplaceStr(qrySub.SQL.Text, '&#60;unitkd&#62;', kd_skpd);&#13;&#10;  qrySub.Open;&#13;&#10;  Result:= (&#60;qrySub.&#34;sub_x&#34;&#62; &#62; 1);&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;FUNCTION TGLINDONESIA(T:TDATETIME):STRING;&#13;&#10;VAR H, B, Y:STRING;&#13;&#10;       M: INTEGER;&#13;&#10;BEGIN&#13;&#10;       H:= '0' + INTTOSTR(DAYOF(T));&#13;&#10;       H:= COPY(H, LENGTH(H)-1,2);&#13;&#10;       M:= MONTHOF(T);&#13;&#10;       CASE M OF&#13;&#10;       1: B:='JANUARI';&#13;&#10;       2: B:='FEBRUARI';&#13;&#10;       3: B:='MARET';&#13;&#10;       4: B:='APRIL';&#13;&#10;       5: B:='MEI';&#13;&#10;       6: B:='JUNI';&#13;&#10;       7: B:='JULI';&#13;&#10;       8: B:='AGUSTUS';&#13;&#10;       9: B:='SEPTEMBER';&#13;&#10;       10: B:='OKTOBER';&#13;&#10;       11: B:='NOPEMBER';&#13;&#10;       12: B:='DESEMBER';&#13;&#10;       END;&#13;&#10;       Y:= INTTOSTR(YEAROF(T));&#13;&#10;       RESULT:= H + ' ' + B + ' ' + Y;&#13;&#10;END;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       QRYTEMP.CLOSE;&#13;&#10;       QRYTEMP.SQL.TEXT:= 'SELECT SUM(A.JUMLAH)JML FROM (' + SQLGOL + ') A';&#13;&#10;       QRYTEMP.OPEN;&#13;&#10;       MBULAN.CLEAR;&#13;&#10;       IF (COBULAN.TEXT&#60;&#62;'') THEN MBULAN.TEXT:= 'BULAN  ' + COBULAN.TEXT + ' TAHUN ' + cbTahun.TEXT&#13;&#10;       ELSE MBULAN.TEXT:= ':..................';&#13;&#10;      { MREKENING.CLEAR;&#13;&#10;       IF (EREKENING.TEXT&#60;&#62;'') THEN MREKENING.TEXT:= ': ' + UPPERCASE(EREKENING.TEXT)&#13;&#10;       ELSE EREKENING.TEXT:= ':..................';&#13;&#10;       MBANK.CLEAR;&#13;&#10;       IF (EBANK.TEXT&#60;&#62;'') THEN MBANK.TEXT:=  ':' + UPPERCASE(EBANK.TEXT)&#13;&#10;       ELSE MBANK.TEXT:= ':..................';&#13;&#10;       MLOKASI.CLEAR;&#13;&#10;       IF (ELOK.TEXT&#60;&#62;'') THEN MLOKASI.TEXT:= ': ' + UPPERCASE(ELOK.TEXT)&#13;&#10;       ELSE MLOKASI.TEXT:= ':..................';&#13;&#10;       MNOMOR.CLEAR;&#13;&#10;       IF (ENOMOR.TEXT&#60;&#62;'') THEN MNOMOR.TEXT:= UPPERCASE(ENOMOR.TEXT)&#13;&#10;       ELSE MNOMOR.TEXT:= '.................................';   }&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       MLAPOR.CLEAR;&#13;&#10;       MLAPOR.TEXT:= 'TANGERANG, ' + TGLINDONESIA(TANGGAL.DATE);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i : integer;&#13;&#10;begin&#13;&#10;  cbTahun.Items.Clear;&#13;&#10;  while not qryTAhun.Eof do&#13;&#10;  begin&#13;&#10;    cbTahun.Items.Add( &#60;qryTAhun.&#34;Tahun&#34;&#62; );&#13;&#10;    qryTAhun.Next;&#13;&#10;  end;&#13;&#10;  cbTahun.ItemIndex := 0;&#13;&#10;&#13;&#10;       TANGGAL.DATE:= DATE;&#13;&#10;       ckSub.Visible:= HasSubUnit(UnitKd);&#13;&#10;       if MonthOf(Now)=12 then coBulan.ItemIndex:=0&#13;&#10;       else coBulan.ItemIndex:=MonthOf(Now);&#13;&#10;      // cbTahun.ItemIndex := 0;&#13;&#10;       //coTahun.Text:=IntToStr(YearOf(Now));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;VAR BLN, THN, JNS, SKPD: STRING;&#13;&#10;begin&#13;&#10;       if rbGaji13.Checked then&#13;&#10;            JNS:= '1'&#13;&#10;       else if rbGajiTerus.Checked then&#13;&#10;               JNS:= '6'&#13;&#10;       else&#13;&#10;               JNS:= '0';&#13;&#10;       IF (CKSUB.CHECKED) THEN&#13;&#10;               SKPD:= 'UNITKD LIKE ''' + UNITKD + '%'''&#13;&#10;       ELSE&#13;&#10;               SKPD:= 'UNITKD=''' + UNITKD + '''';&#13;&#10;       BLN:= '0' + INTTOSTR(COBULAN.ITEMINDEX + 1);&#13;&#10;       BLN:= COPY(BLN, LENGTH(BLN)-1, 2);&#13;&#10;       THN:= cbTahun.TEXT;&#13;&#10;       QRYGOL.CLOSE;&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '99999', JNS);&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '{BULAN}', BLN);&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '{TAHUN}', THN);&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '1=1', SKPD);&#13;&#10;       IF (ENIP.TEXT&#60;&#62;'') THEN&#13;&#10;               REPLACESTR(QRYGOL.SQL.TEXT, '--AND NIP', ' AND (NIP LIKE ''' + ENIP.TEXT + '%'' OR NAMA LIKE ''' + ENIP.TEXT + '%'') ');&#13;&#10;       SQLGOL:= QRYGOL.SQL.TEXT;&#13;&#10;       REPLACESTR(QRYGOL.SQL.TEXT, '---//', '');&#13;&#10;       QRYGOL.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;       QRYTAHUN.OPEN;&#13;&#10;       QRYSKPD.CLOSE;&#13;&#10;       QRYSKPD.PARAMS[0].VALUE:=UNITKD;&#13;&#10;       QRYSKPD.OPEN;&#13;&#10;       QRYProperti.Close;&#13;&#10;       QRYProperti.SQL.Text:= 'SELECT * FROM UNITTAHUN WHERE TAHUN=''' + TahunAnggaran + '''' +&#13;&#10;                       ' AND UnitKd=''' + UnitKd + '''';&#13;&#10;       QRYProperti.Open;&#13;&#10;end." PropData="">
