    <TfrxADOQuery Name="QRYGOL" UserName="QRYGOL" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT GOL.GOLONGAN, ISNULL(PNSA.JML, 0) AS JMLPNS_A, ISNULL(PNSB.JML, 0) AS JMLPNS_B, ISNULL(PNSC.JML, 0) AS JMLPNS_C, ISNULL(PNSD.JML, 0) AS JMLPNS_D,&#13;&#10;	ISNULL(PNSE.JML, 0) AS JMLPNS_E, ISNULL(CPNSA.JML, 0) AS JMLCPNS_A, ISNULL(CPNSB.JML, 0) AS JMLCPNS_B, ISNULL(CPNSC.JML, 0) AS JMLCPNS_C,&#13;&#10;	ISNULL(CPNSD.JML, 0) AS JMLCPNS_D, ISNULL(CPNSE.JML, 0) AS JMLCPNS_E, ISNULL(SIA.JML, 0) AS JMLSI_A,&#13;&#10;	ISNULL(SIB.JML, 0) AS JMLSI_B, ISNULL(SIC.JML, 0) AS JMLSI_C, ISNULL(SID.JML, 0) AS JMLSI_D,&#13;&#10;	ISNULL(SIE.JML, 0) AS JMLSI_E,  ISNULL(AA.JML, 0) AS JMLANAK_A, ISNULL(AB.JML, 0) AS JMLANAK_B, ISNULL(AC.JML, 0) AS JMLANAK_C,&#13;&#10;	ISNULL(AD.JML, 0) AS JMLANAK_D, ISNULL(AE.JML, 0) AS JMLANAK_E,&#13;&#10;	(ISNULL(PNSA.JML, 0) +	ISNULL(PNSB.JML, 0) + ISNULL(PNSC.JML, 0) + ISNULL(PNSD.JML, 0) + ISNULL(PNSE.JML, 0) +&#13;&#10;	ISNULL(CPNSA.JML, 0) +	ISNULL(CPNSB.JML, 0) + ISNULL(CPNSC.JML, 0) + ISNULL(CPNSD.JML, 0) + ISNULL(CPNSE.JML, 0) +&#13;&#10;	ISNULL(SIA.JML, 0) + ISNULL(SIB.JML, 0) + ISNULL(SIC.JML, 0) +	ISNULL(SID.JML, 0) + ISNULL(SIE.JML, 0) +&#13;&#10;	ISNULL(AA.JML, 0) + ISNULL(AB.JML, 0) +	ISNULL(AC.JML, 0) + ISNULL(AD.JML, 0) +	ISNULL(AE.JML, 0)) AS JUMLAH&#13;&#10;FROM&#13;&#10;	(SELECT DISTINCT LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2) GOLONGAN FROM TBLGOLONGAN&#13;&#10;	WHERE NOT URAIAN IS NULL AND NOT RTRIM(GOLONGANKD) LIKE 'V%')GOL&#13;&#10;	LEFT OUTER JOIN&#13;&#10;	(SELECT LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2) GOLONGAN, COUNT(NIP)JML FROM PEGAWAI_GAJI&#13;&#10;	WHERE RIGHT(RTRIM(GOLONGANKD),1)='A'  AND STS_PEGAWAIKD='P'  AND 1=1 AND BULAN='{BULAN}' AND TAHUN='{TAHUN}' AND JENIS=99999 --AND NIP&#13;&#10;	GROUP BY LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2)) PNSA&#13;&#10;	ON GOL.GOLONGAN=PNSA.GOLONGAN&#13;&#10;	LEFT OUTER JOIN&#13;&#10;	(SELECT LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2) GOLONGAN, COUNT(NIP)JML FROM PEGAWAI_GAJI&#13;&#10;	WHERE RIGHT(RTRIM(GOLONGANKD),1)='B'  AND STS_PEGAWAIKD='P'  AND 1=1 AND BULAN='{BULAN}' AND TAHUN='{TAHUN}' AND JENIS=99999 --AND NIP&#13;&#10;	GROUP BY LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2)) PNSB&#13;&#10;	ON GOL.GOLONGAN=PNSB.GOLONGAN&#13;&#10;	LEFT OUTER JOIN&#13;&#10;	(SELECT LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2) GOLONGAN, COUNT(NIP)JML FROM PEGAWAI_GAJI&#13;&#10;	WHERE RIGHT(RTRIM(GOLONGANKD),1)='C'  AND STS_PEGAWAIKD='P'  AND 1=1 AND BULAN='{BULAN}' AND TAHUN='{TAHUN}' AND JENIS=99999 --AND NIP&#13;&#10;	GROUP BY LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2)) PNSC&#13;&#10;	ON GOL.GOLONGAN=PNSC.GOLONGAN&#13;&#10;	LEFT OUTER JOIN&#13;&#10;	(SELECT LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2) GOLONGAN, COUNT(NIP)JML FROM PEGAWAI_GAJI&#13;&#10;	WHERE RIGHT(RTRIM(GOLONGANKD),1)='D'  AND STS_PEGAWAIKD='P'  AND 1=1 AND BULAN='{BULAN}' AND TAHUN='{TAHUN}' AND JENIS=99999 --AND NIP&#13;&#10;	GROUP BY LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2)) PNSD&#13;&#10;	ON GOL.GOLONGAN=PNSD.GOLONGAN&#13;&#10;	LEFT OUTER JOIN&#13;&#10;	(SELECT LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2) GOLONGAN, COUNT(NIP)JML FROM PEGAWAI_GAJI&#13;&#10;	WHERE RIGHT(RTRIM(GOLONGANKD),1)='E'  AND STS_PEGAWAIKD='P'  AND 1=1 AND BULAN='{BULAN}' AND TAHUN='{TAHUN}' AND JENIS=99999 --AND NIP&#13;&#10;	GROUP BY LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2)) PNSE&#13;&#10;	ON GOL.GOLONGAN=PNSE.GOLONGAN&#13;&#10;	LEFT OUTER JOIN&#13;&#10;	(SELECT LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2) GOLONGAN, COUNT(NIP)JML FROM PEGAWAI_GAJI&#13;&#10;	WHERE RIGHT(RTRIM(GOLONGANKD),1)='A'  AND STS_PEGAWAIKD='C'  AND 1=1 AND BULAN='{BULAN}' AND TAHUN='{TAHUN}' AND JENIS=99999 --AND NIP&#13;&#10;	GROUP BY LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2)) CPNSA&#13;&#10;	ON GOL.GOLONGAN=CPNSA.GOLONGAN&#13;&#10;	LEFT OUTER JOIN&#13;&#10;	(SELECT LEFT(RTRIM(GOLONGANKD), LEN(RTRIM(GOLONGANKD))-2) GOLONGAN, COUNT(NIP)JML F